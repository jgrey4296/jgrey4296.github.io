keywords = {bodybuilding,genetic,intensity,meta,muscular strength,norm,phenomena,rule,theory,untagged},
mendeley-tags = {meta,norm,phenomena,rule,theory,untagged},
number = {3},
pages = {147--162},
pmid = {20883897},
title = {{Evidence-Based Resistance Training Recommendations}},
url = {http://versita.metapress.com/openurl.asp?genre=article{\&}id=doi:10.2478/v10036-011-0025-x},
