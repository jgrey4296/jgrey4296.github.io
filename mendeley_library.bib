isbn = {9781450320351},
keywords = {arxiv,expertise,norm,phenomena,recommender systems,rule,theory,untagged,user modeling},
mendeley-tags = {arxiv,norm,phenomena,rule,theory,untagged},
title = {{From Amateurs to Connoisseurs: Modeling the Evolution of User Expertise through Online Reviews}},
