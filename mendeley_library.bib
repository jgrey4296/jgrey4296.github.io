journal = {Proceedings of the 14th International Conference on Autonomous Agents and Multiagent Systems (AAMAS 2015)},
keywords = {MAS,agent,authority,balke,calculus,deontic,higher-order norms,institution,institutions,meta,norm,normative systems,rule,untagged},
mendeley-tags = {MAS,agent,authority,balke,calculus,deontic,institution,meta,norm,rule,untagged},
number = {2},
