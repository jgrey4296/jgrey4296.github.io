file = {:Volumes/DOCUMENTS/MEGA/Mendeley/2003/Winner/Winner{\_}2003{\_}Do Artifacts Have Politics.pdf:pdf;:Volumes/DOCUMENTS/MEGA/Mendeley/2003/Winner/Winner{\_}2003{\_}Do artifacts have politics.pdf:pdf},
isbn = {0335199143 (hc)$\backslash$r0335199135 (pbk.)},
