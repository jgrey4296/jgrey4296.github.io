doi = {10.1007/BF01806174},
file = {:Volumes/DOCUMENTS/MEGA/Mendeley/1988/Wand, Friedman/Wand, Friedman{\_}1988{\_}The mystery of the tower revealed A nonreflective description of the reflective tower.pdf:pdf},
