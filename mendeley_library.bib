issn = {09218890},
journal = {Robotics and Autonomous Systems},
keywords = {MAS,agent,conflicts,cooperation,delegation,help,meta,multi-agent plan,multi-agent systems,rule,theory,typology,unknown,untagged,without},
mendeley-tags = {MAS,agent,delegation,meta,rule,theory,typology,unknown,untagged,without},
