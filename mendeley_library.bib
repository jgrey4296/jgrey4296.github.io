journal = {Computers in Human Behavior},
keywords = {Psychological harm,Sociolotron,Symbolic Taboo Activities,Videogames,Violent video games,World of warcraft,game,meta,phenomena,rule,sanction,theory,untagged},
mendeley-tags = {game,meta,phenomena,rule,sanction,theory,untagged},
number = {1},
pages = {268--275},
publisher = {Elsevier Ltd},
title = {{What i won't do in pixels: Examining the limits of taboo violation in MMORPGs}},
