issn = {1553-7358},
journal = {PLOS Computational Biology},
keywords = {Saliency detection,convolutional neural networks,eye fixation prediction,global context,long short-term memory,rule,scene context},
mendeley-tags = {rule},
month = {sep},
number = {9},
pages = {e1005619},
