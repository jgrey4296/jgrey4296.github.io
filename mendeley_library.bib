number = {1},
pages = {1--49},
title = {{What Is Field Theory ?}},
url = {http://www.jstor.org/stable/10.1086/375201{\%}5Cnhttp://www.csudh.edu/dearhabermas/lewinkurt01.pdf{\%}5Cnhttp://www.jstor.org/discover/10.1086/375201?uid=3738032{\&}uid=2{\&}uid=4{\&}sid=21101393528917},
