mendeley-tags = {Agglomerative hierarchical clustering,Dendrogramix,dendrogram,hybrid visualization,institution,interaction,taxonomy},
title = {{Dendrogramix : a Hybrid Tree-Matrix Visualization Dendrograms}},
year = {2017}
}
