file = {:Volumes/DOCUMENTS/MEGA/Mendeley/2017/Herremans, Chew/Herremans, Chew{\_}2017{\_}MorpheuS generating structured music with constrained patterns and tension.pdf:pdf},
issn = {19493045},
journal = {IEEE Transactions on Affective Computing},
keywords = {Affective Computing,Affective computing applications,Coherence,Computational modeling,Entertainment,Games,Markov processes,Music,Music retrieval and generation,Optimization,Pattern Detection,Rhythm,Sound and Music Computing,Variable Neighborhood Search,agent,arxiv,game,rule,taxonomy,theory},
mendeley-tags = {agent,arxiv,game,rule,taxonomy,theory},
title = {{MorpheuS: generating structured music with constrained patterns and tension}},
