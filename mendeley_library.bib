keywords = {MAS,agent,balke,dagstuhl,game,institution,norm,rule,theory},
mendeley-tags = {MAS,agent,balke,dagstuhl,game,institution,norm,rule,theory},
pages = {17--36},
title = {{Decoupling cognitive agents and virtual environments}},
