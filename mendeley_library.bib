@article{Kotz2018,
abstract = {The recovery from the financial crisis and Great Recession of 2008-09 has been sluggish in the United States and more so in a number of other developed countries. This has given rise to a literature about possible secular stagnation (Gordon 2012, 2014; Krugman 2014; Summers 2014; Ball, DeLong, and Summers 2014; and Ball 2014). This paper proposes an approach to explaining the current stagnation that differs from that found in the above literature. We will argue that the current stagnation stems from the effect of the institutional structure of the economy on long-run macroeconomic performance. This explanation for the current stagnation is based on the social structure of accumulation (SSA) theory (Gordon, 1980; Gordon, Edwards, and Reich, 1982, chapter 2; Kotz et al 1994; and McDonough et al, 2010), which is explained below.},
author = {Kotz, David M and Basu, Deepankar},
doi = {10.1177/0486613416673472},
file = {:Volumes/DOCUMENTS/MEGA/Mendeley/2018/Kotz, Basu/Kotz, Basu{\_}2018{\_}Stagnation and Institutional Structures.pdf:pdf},
issn = {0486-6134},
journal = {Review of Radical Political Economics},
keywords = {accumulation,institution,norm,phenomena,rule,social structure of accumulation,stagnation,theory},
mendeley-tags = {institution,norm,phenomena,rule,theory},
month = {sep},
pages = {048661341667347},
title = {{Stagnation and Institutional Structures}},
url = {http://journals.sagepub.com/doi/10.1177/0486613416673472},
year = {2018}
}
@inproceedings{Balas2004,
abstract = {Petri Nets can be used for a retrospective analysis of a computer game$\backslash$nstory, for representing plots in serious games as well as for$\backslash$nmonitoring the course of the story, as recently demonstrated by several$\backslash$nauthors. It was not clear, however, whether Petri Nets can be used for$\backslash$nrepresenting plots and for their unfolding in games that feature large$\backslash$nworlds inhabited by virtual humans. This paper demonstrates that this$\backslash$nis indeed possible, presenting several scenarios from the serious game$\backslash$nKaro, an ongoing project, which features both virtual humans driven by$\backslash$nreactive planning and a story manager unfolding story plots represented$\backslash$nand controlled by Petri Nets. However, a specific refinement of Petri$\backslash$nNets, a hierarchical model capitalising on Timed Coloured Petri Nets$\backslash$nand non-deterministic FSMs, had to be developed for this purpose. This$\backslash$nrefinement is described here, including general discussion showing why$\backslash$nindividual features of this refinement are needed - directions for game$\backslash$ndevelopers considering whether to use this technique, or not.},
author = {Balas, Daniel and Brom, Cyril and Abonyi, Adam and Gemrot, Jakub},
booktitle = {AIIDE},
