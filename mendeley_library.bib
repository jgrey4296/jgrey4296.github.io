@article{Pitt2015,
abstract = {Many open networks, distributed computing systems, and infrastructure management systems face a common problem: how to distribute a collectivised set of resources amongst a set of autonomous agents of heterogenous provenance. One approach is for the agents themselves to self-organise the allocation of resources with respect to a set of agreed conventional rules; but given an allocation scheme which maps resources to those agents and a set of rules for determining that allocation scheme, some natural questions arise-Is this allocation fair? Is the allocation method effective? Is it efficient? Are the decision makers accountable? In this paper, we argue that some answers to these questions can be found in the formal characterisation of different aspects of 'justice' and that these different aspects need a principled operationalisation as policies for system management. We present a formal model and some experimental results, concluding that the different aspects are all inter-connected and that what is required is a comprehensive research programme in computational justice. {\textcopyright} 2013 Springer-Verlag London.},
author = {Pitt, Jeremy and Busquets, D{\'{i}}dac and Riveret, R{\'{e}}gis},
doi = {10.1007/s00146-013-0531-6},
file = {:Volumes/DOCUMENTS/MEGA/Mendeley/2015/Pitt, Busquets, Riveret/Pitt, Busquets, Riveret{\_}2015{\_}The pursuit of computational justice in open systems.pdf:pdf;:Volumes/DOCUMENTS/MEGA/Mendeley/2015/Pitt, Busquets, Riveret/Pitt, Busquets, Riveret{\_}2015{\_}The pursuit of computational justice in open systems(2).pdf:pdf},
issn = {14355655},
journal = {AI and Society},
keywords = {Computational justice,MAS,Multi-agent systems,Resource allocation,Self-organisation,agent,balke,calculus,institution,meta,norm,rule,sanction,taxonomy,theory},
mendeley-tags = {MAS,agent,balke,calculus,institution,meta,norm,rule,sanction,taxonomy,theory},
number = {3},
pages = {359--378},
title = {{The pursuit of computational justice in open systems}},
volume = {30},
year = {2015}
}
