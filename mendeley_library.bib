isbn = {0003-3472 (Print)$\backslash$r0003-3472},
issn = {00033472},
journal = {Animal Behaviour},
keywords = {Altruism,Chimpanzee,Food sharing,Pan troglodytes,Prosociality,Reciprocity,Tolerated theft,calculus,institution,norm,untagged},
mendeley-tags = {calculus,institution,norm,untagged},
