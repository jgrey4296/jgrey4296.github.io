keywords = {arxiv,haskell,rule,theory},
mendeley-tags = {arxiv,haskell,rule,theory},
pages = {1--14},
title = {{Loop Patterns: Extension of Kleene Star Operator for More Expressive Pattern Matching against Arbitrary Data Structures}},
