@article{Robol2016,
abstract = {Smart cities are founded on complex interactions among architectural and urban designs, sensors, actuators and crowds of people with their devices.$\backslash$r$\backslash$nIn this context, simulation becomes essential to study the effects of the technology and to understand how to improve its effectiveness on the social environment.$\backslash$r$\backslash$nThe majority of the current pedestrian and traffic simulations adopt a bird-eye view and are driven by statistical models.$\backslash$r$\backslash$nWhile this is enough in many cases, e.g. to study traffic flow under common conditions assuming average cases, it is not appropriate when a higher level of fidelity is required.$\backslash$r$\backslash$nSimulated people need to show both a plausible behavior and mechanisms to coordinate with human participants in a natural way.$\backslash$r$\backslash$nMuch of this coordination happens silently and is driven by social norms, that may vary according to culture and context.$\backslash$r$\backslash$nIn this paper, we propose an approach to represent social norms in multi-agent systems that enables implicit coordination driven by observations of others' behaviors.$\backslash$r$\backslash$nThis is applied specifically to the case of pedestrian movement.$\backslash$r$\backslash$nIn order to allow for a more effective participation of humans in the simulation, our approach does not use central coordinators or coordination protocol, but rather each agent takes its own decision so to make more realistic interactions.$\backslash$r$\backslash$nA software architecture and initial experimental results are presented and discussed.},
author = {Robol, Marco and Giorgini, Paolo and Busetta, Paolo},
doi = {10.1109/ISC2.2016.7580808},
file = {:Volumes/DOCUMENTS/MEGA/Mendeley/2016/Robol, Giorgini, Busetta/Robol, Giorgini, Busetta{\_}2016{\_}Applying social norms to high-fidelity pedestrian and traffic simulations.pdf:pdf},
isbn = {9781509018451},
journal = {IEEE 2nd International Smart Cities Conference: Improving the Citizens Quality of Life, ISC2 2016 - Proceedings},
keywords = {MAS,agent,delegation,game,meta,norm,rule,theory,untagged},
mendeley-tags = {MAS,agent,delegation,game,meta,norm,rule,theory,untagged},
title = {{Applying social norms to high-fidelity pedestrian and traffic simulations}},
year = {2016}
}
