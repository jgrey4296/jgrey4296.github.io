keywords = {arxiv,calculus,untagged},
mendeley-tags = {arxiv,calculus,untagged},
pages = {i--x 1--303},
title = {{Book of Proof}},
url = {http://www.people.vcu.edu/{~}rhammack/BookOfProof/},
year = {2013}
}
