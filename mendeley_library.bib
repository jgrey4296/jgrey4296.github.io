@article{Gebser2015,
abstract = {This document provides an introduction to the Answer Set Programming (ASP) tools gringo, clasp, and clingo, developed at the University of Potsdam. The ba- sic idea of ASP is to express a problem in the form of a logic program so that its logical models, called answer sets, provide the solutions to the original problem. The first tool, gringo, is a so-called grounder translating user-provided logic programs (with variables) into equivalent propositional logic programs (without variables). The second tool, clasp, is a so-called solver computing the answer sets of the propositional programs issued by gringo. The third tool, clingo, combines the functionalities of gringo and clasp, and additionally integrates the scripting languages lua and python either through libraries or embedded code. This guide, for one, aims at enabling ASP novices to make use of the aforementioned tools. For another, it provides a reference of the tools' features that ASP adepts might be tempted to exploit. The guide generally refers to gringo and clingo series 4 as well as clasp series 3; full conformity is obtained with gringo 4.5, clingo 4.5 and clasp 3.1. Please make sure that you have corresponding (or later) versions available.},
archivePrefix = {arXiv},
