file = {:Volumes/DOCUMENTS/MEGA/Mendeley/1998/Szalav{\'{a}}ri, Eckstein, Gervautz/Szalav{\'{a}}ri, Eckstein, Gervautz{\_}1998{\_}Collaborative gaming in augmented reality.pdf:pdf},
