doi = {10.1016/j.chb.2011.11.022},
file = {:Volumes/DOCUMENTS/MEGA/Mendeley/2012/Orr, Ross, Robert Orr/Orr, Ross, Robert Orr{\_}2012{\_}Trait and symptom differences between factions in online gaming The vulnerable side of evil.pdf:pdf},
