file = {:Volumes/DOCUMENTS/MEGA/Mendeley/2001/Brewka/Brewka{\_}2001{\_}Dynamic argument systems A formal model of argumentation processes based on situation calculus.pdf:pdf},
issn = {0955792X},
journal = {Journal of Logic and Computation},
keywords = {Argumentation,Defeasible reasoning,Dynamic protocols,MAS,agent,authority,calculus,deontic,meta,norm,rule,theory},
mendeley-tags = {MAS,agent,authority,calculus,deontic,meta,norm,rule,theory},
