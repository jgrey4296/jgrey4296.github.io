keywords = {MAS,Papers,agent,game,mateas,untagged,without},
mendeley-tags = {MAS,Papers,agent,game,mateas,untagged,without},
pages = {278--291},
title = {{An objective character believability evaluation procedure for multi-agent story generation systems}},
volume = {3661 LNAI},
year = {2005}
}
