number = {2},
pages = {1--24},
title = {{Character play-The use of game characters in multi-player role-playing games across platforms}},
url = {http://epress.lib.uts.edu.au/research/handle/10453/15102 http://doi.acm.org/10.1145/1371216.1371225},
volume = {6},
