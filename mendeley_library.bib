issn = {1741-7007},
journal = {BMC Biology},
keywords = {Sex-differential expression,Sex-differential selec,game,meta,phenomena,sex-differential expression,sex-differential selection,sexual dimorphism,theory,untagged},
mendeley-tags = {game,meta,phenomena,theory,untagged},
number = {1},
pages = {7},
pmid = {28173793},
publisher = {BMC Biology},
title = {{The landscape of sex-differential transcriptome and its consequent selection in human adults}},
