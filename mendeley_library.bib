keywords = {agent,authority,bourdieu,husserl,institution,phenomena,ritual,rule,sanction,theory},
mendeley-tags = {agent,authority,bourdieu,husserl,institution,phenomena,ritual,rule,sanction,theory},
title = {{Varieties of Sociological Field Theory}},
url = {http://link.springer.com/10.1007/978-3-319-32250-6},
year = {2016}
}
@book{Shegog2010,
