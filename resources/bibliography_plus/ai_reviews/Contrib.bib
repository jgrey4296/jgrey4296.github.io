@inproceedings{mugellini_wiinote:_2009,
 abstract     = {In this paper, we describe a multimodal application, called {WiiNote,} facilitating multi-user photo annotation activity. The application allows up to 4 users to simultaneously annotating their pictures adding either textual or vocal comments. Users use the Wii Remote device to select the whole picture or a specific region of it to be annotated. Annotations can be either free or structured, i.e. based on a domain specific data model expressed using {MPEG7} standard or {RDF} language for ontology.},
 address      = {Cambridge, Massachusetts, {USA},
