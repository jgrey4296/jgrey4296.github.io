 title        = {El curr{\i}́culo de la Inform{\'a}tica en la Ley Org{\'a}nica de Educaci{\'o}n Espa{\~n}ola},
 year         = {2006},
 journal      = {Informatica Educativa Comunicaciones},
 tags         = {__untagged},
 abstract     = {La ense{\~n}anza de la Inform{\'a}tica de la Educaci{\'o}n Secundaria est{\'a} desperdigada en la asignatura de Tecnolog{\i}́a en lugar de estar definida como materia independiente, lo cual viene desencadenando los siguientes efectos: se inhabilita al profesorado de Inform{\'a}tica -a quien no corresponde impartir la asignatura de tecnolog\{\'a}- en sus funciones, se habilitan encubiertamente para ello a quienes no acreditan esa formaci{\'o}n y, finalmente, se perjudica -por gusto- la calidad de su ense{\~n}anza. El d{\'e}ficit y la exclusi{\'o}n de estos conocimientos y docentes merma las posibilidades de utilizaci{\'o}n de las Tecnolog\'{\' s} de la Informaci{\'o}n y la Comunicaci{\'o}n (TIC) en todas las {\'a}reas. As\'{\i}{\' s} como el Gobierno le causa, sin m{\'a}s, un lastre al Pa\'{\i}s ́un subdesarrollo para su desenvolvimiento en esta sociedad del conocimiento del siglo XXI.},
 bdsk-url-1   = {http://www.adie.es/iecom/index.php/IECom/article/view/19},
 date-added   = {2009-03-02 19:56:11 +0100},
 date-modified= {2009-03-02 19:56:11 +0100},
 issn         = {1699-4574},
 keywords     = {Educaci{\'o}n Secundaria; Curr{\i}́culo; Inform{\'a}tica; Tecnolog\{\'a}s de la Informaci{\'o}n y la Comunicaci{\'o}n (TIC); Sociedad del Conocimiento; Ley Org{\'a}nica de Educaci{\'o}n (LOE).},
 number       = {4},
 url          = {http://www.adie.es/iecom/index.php/IECom/article/view/19},
 volume       = {2},
}

@article{IECom20,
