 keywords     = {Colaboraci{\'o}n; aprendizaje colaborativo; integraci{\'o}n; dise{\~n}o instruccional},
 number       = {6},
 url          = {http://www.adie.es/iecom/index.php/IECom/article/view/7},
 volume       = {3},
}

