 language     = {en},
 location     = {Vrije Universiteit Amsterdam, The Netherlands},
 month        = {dec~12},
 publisher    = {Yorktown Heights: IBM Research Devision},
 series       = {IBM Research Report},
 tags         = {QoS,Quality-Aware,Web Service Discovery},
 title        = {An Ontology for Quality-Aware Service Discovery},
 url          = {http://www.it-weise.de/documents/files/BW2005QASD.pdf},
 volume       = {RC23821},
 year         = {2005},
}

