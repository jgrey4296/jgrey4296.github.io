 language     = {en},
 month        = {jul~2},
 number       = {2007, 2},
 organization = {University of Kassel},
 pages        = {1--8},
 series       = {Kasseler Informatikschriften (KIS)},
 tags         = {Distributed Algorithms,GP,Genetic Programming,MDA,MDD,MOF-Skript,Model Driven Architecture,Model Driven Development,UML,XMI},
 title        = {Genetic Programming meets Model-Driven Development},
