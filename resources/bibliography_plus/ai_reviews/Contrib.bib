 ee           = {http://dx.doi.org/10.1007/978-3-642-01338-6_5},
 keywords     = {AOSE, Development Process, Methodologies, Model Driven, SPEM},
 pages        = {60--73},
 publisher    = {Springer Verlag},
 series       = {Lecture Notes in Computer Science},
}

