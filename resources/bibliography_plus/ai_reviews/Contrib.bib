@article{bussmann:1993:CA,
 abstract     = {Coperation in societies of intelligent units (agents) is almost always established on account of communication between agents. Hence the agents must be equipped with a communication module that allows them to take active part in the negotiation process of the society. Further there have to be a set of keywords and a protocol which define a kind of interlingua. After a detailed motivation, the role of speech acts as a basis for a communication architecture for cooperating agents will be discussed. Viewing them as actions the agents have to perform, the planning aspects will be elaborated. A formal model of communication will then be given which leads, together with the speech act planning, to the communication architecture.},
 author       = {S. Bussmann and J. M{\"u}ller},
 journal      = {Computers and Artificial Intelligence},
 number       = {1},
 pages        = {37--53},
 tags         = {communication,multi-agent systems},
 title        = {A Communication Architecture for Cooperating Agents},
 volume       = {12},
 year         = {1993},
}

@incollection{bussmann:1993:NF,
 abstract     = {Multi-Agent worlds differ from other domains by the fact that the inhabiting agents have to interact to reach their local goals. Hence, communication (especially negotiation) is the basic action for the agents in order to coordinate their activities. Starting with a general discussion on negotiation aspects and forms of negotiation behaviour a negotiation model will be developed. The negotiation algorithm will be described and the 'Travelling Plan' negotiation domain will demonstrate the application of the introduced concepts. In order to compare the proposed model with other approaches (especially from Distributed Problem Solving) a summary of current negotiation methods will be given.},
 author       = {S. Bussmann and J. M{\"u}ller},
 booktitle    = {1992 Proceedings of the Special Interest Group on Cooperating Knowledge-Based Systems},
 editor       = {S. M. Deen},
 pages        = {1--17},
 publisher    = {Dake Centre, University of Keele},
 tags         = {multi-agent systems,negotiation},
 title        = {A Negotiation Framework for Cooperating Agents},
 year         = {1993},
}

@incollection{bussmann:1996:DAS,
 abstract     = {Advanced manufacturing control still remains an important topic in current research. Especially aspects of dynamics and failures in the production process are insufficiently taken into account by systems in use. This paper presents a multi-agent approach to scheduling material flow that shows dynamic and adaptive behaviour. Even though machine scheduling has found a thorough treatment in AI literature, there are only few investigations on the material flow problem. In this paper, it is argued that a decentralized architecture with centralized control fits well with the local and global aspects of the scheduling problem. The top-level algorithms of the scheduling process are outlined and further improvements required are sketched out.},
 author       = {S. Bussmann},
 booktitle    = {Distributed Software Agents and Applications},
 editor       = {J. W. Perram and J.-P. M{\"u}ller},
 number       = {1069},
 pages        = {191--205},
 publisher    = {Springer Verlag, Berlin},
 series       = {Lecture Notes in Computer Science},
 tags         = {manufacturing,multi-agent systems,scheduling},
 title        = {A Multi-Agent Approach to Dynamic, Adaptive Scheduling of Material Flow},
 year         = {1996},
}

