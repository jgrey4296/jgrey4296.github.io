@inproceedings{GaudHilaireGallandKoukamCossentino2008_15,
 author       = {Gaud, Nicolas and Hilaire, Vincent and Galland, St{\'e}phane and Koukam, Abderrafiaa and Cossentino, Massimo},
 editor       = {Bernhard Jung and Fabien Michel and Alessandro Ricci and Paolo Petta},
 title        = {A Verification by Abstraction Framework for Organizational Multiagent Systems},
 year         = {2008},
 booktitle    = {the Sixth International Workshop AT2AI-6: "From Agent Theory to Agent Implementation", of the Seventh International Conference on Autonomous agents and Multiagent Systems (AAMAS)},
 tags         = {__untagged},
 abstract     = {Software agents and multi-agent systems (MAS from now on) are recognized as both abstractions and effective technologies for modelling and building complex distributed applications. However, they are still difficult to engineer. The reason is that when massive number of autonomous components interact it is very difficult to predict that the emergent organizational structure fits the system goals or that the desired functionalities will be fulfilled. Verification approaches try to evaluate whether or not a product, service, or system complies with a specification. However verification approaches are limited by the state- space of the system under study. This paper proposes an approach based upon an organizational framework and specifically the capacity concept which enables to abstract a role know-how and to reduce the state space of the system under study. A formal framework based on multi-formalisms language and the specification approach are presented and illustrated through the specification of a part of the contract net protocol.},
 address      = {Estoril, Portugal},
 keywords     = {Agent Oriented Software Engineering, Verification, Abstraction},
 month        = {may},
 pages        = {67--73},
}

