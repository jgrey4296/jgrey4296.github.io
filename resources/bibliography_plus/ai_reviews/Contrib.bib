 tags         = {Critical Section,Distributed Systems,Epistasis,GP,Genetic Programming,Learning Classifier Systems,Neutrality,Rule-based Genetic Programming},
 title        = {Rule-based Genetic Programming},
 url          = {http://www.it-weise.de/documents/files/WZG2007RBGP.pdf},
