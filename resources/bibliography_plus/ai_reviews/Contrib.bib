 journal      = {Informatica Educativa Comunicaciones},
 tags         = {__untagged},
 abstract     = {Los Lenguajes de Modelado Educativo (EMLs, atendiendo a sus siglas en ingl{\'e}s) han sido propuestos para permitir el dise{\~n}o instruccional de materiales de e-learning. Estos lenguajes se pueden englobar en el movimiento de estandarizaci{\'o}n de e-learning cuyo objetivo es permitir el modelado de materiales educativos en unidades reutilizables e interoperables. Las propuestas existentes hasta este momento se desarrollaban entorno al concepto de Objeto de Aprendizaje, concebido como unidad b{\'a}sica de reutilizaci{\'o}n basada en el empaquetado de materiales educativos, pero en los que no se ten{\i}́an en cuenta cuestiones pedag{\'o}gicas o instructivas. Los EMLs se caracterizan por permitir modelar la utilizaci{\'o}n instructiva de los materiales educativos considerando la forma en que los aprendices acceden al contenido, la interacci{\'o}n entre los participantes (otros alumnos y profesores), etc. Actualmente, IMS Learning Design es la propuesta de EML m{\'a}s destacada. Este ar{\c t}́ulo considera tres aspectos principales que deber\'{\'n} considerarse en los EMLs (generalidad, completitud y flexibilidad) y presenta una propuesta para realzar el soporte de dise{\~n}o instruccional.},
 bdsk-url-1   = {http://www.adie.es/iecom/index.php/IECom/article/view/25},
 date-added   = {2009-03-02 20:02:34 +0100},
 date-modified= {2009-03-02 20:02:34 +0100},
 issn         = {1699-4574},
 keywords     = {e-learning; aprendizaje colaborativo; lenguajes de modelado educativo; unidad de aprendizaje; objeto de aprendizaje.},
 number       = {3},
 url          = {http://www.adie.es/iecom/index.php/IECom/article/view/25},
 volume       = {2},
}

@article{IECom26,
 author       = {Celorrio, Carlos and Verdejo, Maria and Blanco, Beatriz Barros},
 title        = {Un Modelo de Distribuci{\'o}n de Repositorios para un Portal de Comunidades de Aprendizaje Colaborativo},
 year         = {2006},
 journal      = {Informatica Educativa Comunicaciones},
 tags         = {__untagged},
 abstract     = {La distribuci{\'o}n de contenidos y/o metadatos en Repositorios de Objetos de Aprendizaje es un aspecto clave a la hora de favorecer el intercambio de conocimiento dentro de comunidades de aprendizaje. En este art{\i}́culo se presenta una soluci{\'o}n basada en servicios web para ofrecer esta distribuci{\'o}n. Partiendo del an{\'a}lisis de las necesidades de una comunidad para compartir recursos generados en distintos escenarios tem{\'a}ticos cient\f́ico-t{\'e}cnicos, se propone una soluci{\'o}n que cumple sus requisitos pedag{\'o}gicos. Se discuten las distintas opciones tecnol{\'o}gicas disponibles y se presenta el dise{\~n}o realizado, que se caracteriza adem{\'a}s por ofrecer interoperabilidad y flexibilidad para adaptar la configuraci{\'o}n a diferentes situaciones.},
 bdsk-url-1   = {http://www.adie.es/iecom/index.php/IECom/article/view/26},
 date-added   = {2009-03-02 20:04:00 +0100},
 date-modified= {2009-03-02 20:04:00 +0100},
 issn         = {1699-4574},
 keywords     = {Repositorios de objetos de aprendizaje; distribuci{\'o}n; servicios web.},
 number       = {3},
 url          = {http://www.adie.es/iecom/index.php/IECom/article/view/26},
 volume       = {2},
}

@article{IECom27,
 author       = {Romero, Crist{\'o}bal and Ventura, Sebasti{\'a}n and Herv{\'a}s, C{\'e}sar and R{\i}́der, Isaac and Mart\{\'n}-Palomo, Santiago},
 title        = {Herramienta Autor para la Gesti{\'o}n de Tests Informatizados dentro del Sistema AHA!},
 year         = {2006},
 journal      = {Informatica Educativa Comunicaciones},
 tags         = {__untagged},
 abstract     = {En este art{\i}́culo presentamos Test Editor, una herramienta autor para la construcci{\'o}n de test informatizados, tanto cl{\'a}sicos como adaptativos, a trav{\'e}s del Web. Esta herramienta facilita el desarrollo y mantenimiento de diferentes tipos de test de tipo multi-opci{\'o}n o multi-respuesta, con el objetivo de poder utilizarlos dentro de sistemas educativos basados en web. Test editor es una herramienta modular que permite configurar varios par{\'a}metros sobre las preguntas o \t́ems y los tests. Tambi{\'e}n proporciona informaci{\'o}n estad\'t́ica sobre la utilizaci{\'o}n de los tests, que puede ser utilizada para el mantenimiento de los tests. Esta herramienta se ha integrado dentro del sistema AHA!, pero se puede utilizar tambi{\'e}n dentro de otros sistemas basados en web. Para probar el funcionamiento de la herramienta se ha utilizado en la creaci{\'o}n de un test adaptativo para evaluar a dos grupos de alumnos de un mismo curso de extensi{\'o}n universitaria sobre programaci{\'o}n en el lenguaje Java.},
 bdsk-url-1   = {http://www.adie.es/iecom/index.php/IECom/article/view/27},
 date-added   = {2009-03-02 20:05:54 +0100},
 date-modified= {2009-03-02 20:05:54 +0100},
 issn         = {1699-4574},
 keywords     = {Tests Informatizados Adaptativos; Sistemas para Educaci{\'o}n basados en Web.},
 number       = {3},
 url          = {http://www.adie.es/iecom/index.php/IECom/article/view/27},
 volume       = {2},
}

@article{IECom28,
 author       = {Kerejeta, Mikel and Larra{\~n}aga, Mikel and Rueda, Urko and Urrutia, Ainara and Arruarte, Ana and Arandia, Jon Elorriaga},
 title        = {Una aproximaci{\'o}n multiling{\"u}e a la evaluaci{\'o}n basada en la web},
 year         = {2006},
 journal      = {Informatica Educativa Comunicaciones},
 tags         = {__untagged},
 abstract     = {A lo largo de este art{\i}́culo se presenta TOKA, un sistema web de evaluaci{\'o}n que permite generar ejercicios multiling{\"u}es de tipo test y evaluarlos autom{\'a}ticamente. TOKA ha sido dise{\~n}ado siguiendo el paradigma de internacionalizaci{\'o}n/localizaci{\'o}n y permite gestionar dominios en idiomas diferentes de forma integrada. La herramienta puede ser utilizada tanto dirigida por el profesor, que genera y asigna ex{\'a}menes, como dirigida por el propio estudiante, en procesos de auto-evaluaci{\'o}n. En el ar{\c t}́ulo tambi{\'e}n se describe una experiencia de utilizaci{\'o}n de TOKA en un entorno educativo real.},
 bdsk-url-1   = {http://www.adie.es/iecom/index.php/IECom/article/view/28},
 date-added   = {2009-03-02 20:06:35 +0100},
 date-modified= {2009-03-02 20:06:35 +0100},
 issn         = {1699-4574},
 keywords     = {Evaluaci{\'o}n basada en la web; Herramientas de aprendizaje multiling{\"u}es; Tests.},
 number       = {3},
 url          = {http://www.adie.es/iecom/index.php/IECom/article/view/28},
 volume       = {2},
}

@article{IECom29,
 author       = {Mart{\i}́n, Bego{\~n}a Ferrero},
 title        = {DETECTive},
 subtitle     = {Un entorno gen{\'e}rico e integrable para diagn{\'o}stico de actividades de aprendizaje. Fundamentos, dise{\~n}o y evaluaci{\'o}n},
 year         = {2005},
 journal      = {Informatica Educativa Comunicaciones},
 tags         = {__untagged},
 abstract     = {Esta tesis trata el proceso de diagn{\'o}stico en los sistemas educativos proponiendo un modelo de tareas y un modelo de diagn{\'o}stico gen{\'e}rico aplicable a distintos dominios. Ambos modelos combinan varias t{\'e}cnicas para favorecer el proceso de adquisici{\'o}n de conocimiento, y ofrecer un diagn{\'o}stico adecuado y fiable a partir de la revisi{\'o}n de las respuestas de los estudiantes a los ejercicios que se les plantean. El trabajo se ha implementado en el sistema DETECTive, el cual puede utilizarse sobre diferentes dominios e integrarse con otras aplicaciones. DETECTive ha sido probado en el dominio de la m{\'a}quinaherramienta para construir el sistema FROGA-LAN.},
 bdsk-url-1   = {http://www.adie.es/iecom/index.php/IECom/article/view/29},
 date-added   = {2009-03-02 20:11:58 +0100},
 date-modified= {2009-03-02 20:11:58 +0100},
 issn         = {1699-4574},
 keywords     = {Diagn{\'o}stico educativo; Modelo de Tareas; Seguimiento de modelos.},
 number       = {2},
 url          = {http://www.adie.es/iecom/index.php/IECom/article/view/29},
 volume       = {1},
}

@article{IECom3,
 author       = {Rif{\'o}n, Luis Anido and Santos, Juan and M{\i}́guez, Rub{\'e}n},
 title        = {Introducci{\'o}n de las TIC en la educaci{\'o}n infantil de 0 a 3 a{\~n}os},
 year         = {2007},
 journal      = {Informatica Educativa Comunicaciones},
 tags         = {__untagged},
 abstract     = {Este art{\i}́culo presenta los primeros resultados del proyecto ``galescolas.net'' realizado por la Universidad de Vigo para la Secretar\{\'a} Xeral da Igualdade de la Vicepresidencia da Igualdade e do Benestar de la Xunta de Galicia. El objetivo fundamental es iniciar el proceso de introducci{\'o}n de las Tecnolog\'{\' s} de la Informaci{\'o}n y Comunicaci{\'o}n (TIC) en la educaci{\'o}n infantil para el rango de edades comprendido entre los 0 y los 3 a{\~n}os. Se realiza una introducci{\'o}n de las principales funcionalidades ofrecidas, analizando las primeras reacciones por parte de los educadores infantiles. Asimismo se muestra la metodolog\'{\i}d́e trabajo utilizada en su desarrollo y la colaboraci{\'o}n entre los equipos de tecn{\'o}logos, pedagogos y gestores de la administraci{\'o}n aut{\'o}noma de Galicia.},
 bdsk-url-1   = {http://www.adie.es/iecom/index.php/IECom/article/view/3},
