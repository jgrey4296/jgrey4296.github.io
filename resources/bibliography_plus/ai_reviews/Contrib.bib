 date-modified= {2009-03-02 19:43:27 +0100},
 issn         = {1699-4574},
 number       = {6},
 url          = {http://www.adie.es/iecom/index.php/IECom/article/view/3},
 volume       = {3},
}

@article{IECom30,
 author       = {L{\'o}pez-Cuadrado, Javier and Fern{\'a}ndez, Joaqu{\i}́n S{\'a}nchez},
 title        = {GenTAI},
 subtitle     = {Generador de tests adaptativos informatizados},
 year         = {2005},
 journal      = {Informatica Educativa Comunicaciones},
 tags         = {__untagged},
 abstract     = {Este art{\i}́culo presenta una herramienta que permite la generaci{\'o}n autom{\'a}tica de Tests Adaptativos Informatizados experimentales o a peque{\~n}a escala a partir de un banco de \t́ems que est{\'e} calibrado seg{\'u}n el modelo log\'t́ico de 3 par{\'a}metros de la Teor\'{\i}d́e Respuesta al {\'I}tem. GenTAI es un software amigable, de f{\'a}cil uso, intuitivo, portable, escalable, altamente configurable y fuertemente documentado, que al mismo tiempo da respuesta a los objetivos de un test adaptativo. Otros sistemas similares pecan en la calidad de sus interfaces y en la forma de interactuar con el usuario. Como novedad, GenTAI incluye un m{\'o}dulo que permite al usuario elaborar simulaciones para evaluar o comparar los errores de medida obtenidos tras la aplicaci{\'o}n de los tests.},
 bdsk-url-1   = {http://www.adie.es/iecom/index.php/IECom/article/view/30},
 date-added   = {2009-03-02 20:07:20 +0100},
 date-modified= {2009-03-02 20:07:20 +0100},
 issn         = {1699-4574},
 keywords     = {tests adaptativos informatizados; bancos de {\i}́tems; herramientas de evaluaci{\'o}n.},
 number       = {2},
 url          = {http://www.adie.es/iecom/index.php/IECom/article/view/30},
 volume       = {1},
}

@article{IECom31,
 author       = {Vera, Ra{\'u}l Aguilar and de Antonio Jim{\'e}nez, Ang{\'e}lica},
 title        = {Entrenamiento de equipos},
 subtitle     = {Una estrategia asistida por entornos virtuales inteligentes},
 year         = {2005},
 journal      = {Informatica Educativa Comunicaciones},
 tags         = {__untagged},
 abstract     = {El uso de Entornos Virtuales Inteligentes (EVI's) como herramienta de apoyo a la educaci{\'o}n representa un intento por ofrecer un modelo ecl{\'e}ctico para los esquemas de formaci{\'o}n que han asistido de manera independiente a los procesos de ense{\~n}anza y aprendizaje. En este trabajo se describe una estrategia de entrenamiento para peque{\~n}os grupos, la cual se compone de cuatro fases interrelacionadas en las que se propone la utilizaci{\'o}n de un entorno virtual colaborativo. Se considera la inclusi{\'o}n de un tutor humano para aquellas fases en las que las actividades grupales giren en torno a una reuni{\'o}n virtual, as{\i}́ como un Agente Virtual Pedag{\'o}gico con el rol de l\d́er del grupo durante la fase de ejecuci{\'o}n de la tarea propuesta.},
 bdsk-url-1   = {http://www.adie.es/iecom/index.php/IECom/article/view/31},
 date-added   = {2009-03-02 20:07:58 +0100},
 date-modified= {2009-03-02 20:07:58 +0100},
 issn         = {1699-4574},
 keywords     = {Entornos Virtuales Inteligentes; Entornos Virtuales Colaborativos; Sistemas Inteligentes de Tutor{\i}́a; Entrenamiento de Equipos; Agente Pedag{\'o}gico; Realidad Virtual.},
 number       = {2},
 url          = {http://www.adie.es/iecom/index.php/IECom/article/view/31},
 volume       = {1},
}

