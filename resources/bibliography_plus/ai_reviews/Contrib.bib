 volume       = {1},
}

@article{IECom34,
 author       = {Pulido, Juan Antonio G{\'o}mez and del Solar, Manuel Rubio and P{\'e}rez, Juan S{\'a}nchez and Rodr{\i}́guez, Miguel Vega},
