@phdthesis{G.Castillo:06,
 abstract     = {This thesis mainly addresses the development of adaptive learning algorithms for Bayesian network Classifiers (BNCs) in an on-line leaning scenario. An efficient adaptive algorithm in an on-line learning scenario must be able, above all, to improve its predictive accuracy over time while reducing the cost of adaptation. However, in many real-world situations it may be difficult to improve and adapt to existing changing environments, a problem known as concept drift. We have integrated all the adaptive algorithms into an adaptive prequential (on-line) framework for supervised learning called AdPreqFr4SL, which attempts to handle the {\em cost-performance trade-off} and {\em concept drift}. The cost-quality trade-off is approached through {\em bias management} and {\em adaptation control}. Instead of selecting a particular class of BNCs and using it during all the learning process, we start with the simple Na{{\"}}ve Bayes. We can then improve the performance of Na{{\"}}ve Bayes over time if we reduce the bias resulting from the independence assumption by gradually adding dependencies between the attributes over time. To this end, we gradually increase the maximum number of allowable attribute dependences, so that at each learning step we can use a class-model of BNCs that better suits the available data. On the other hand, since updating the structure of BNCs with new data is a very costly task, we reduce this cost by using new data to primarily adapt the parameters. Only when it is detected that the use of the current structure no longer guarantees the desirable improvement in the performance, do we adapt the structure. To handle concept drift learning algorithms should be provided with some control and adaptive mechanisms that effort to adjust quickly to these changes. Our framework includes a method based on Statistical Quality Control, which has been demonstrated to be efficient for handling concept changes. We experimentally evaluated the AdPreqFr4SL on artificial domains and benchmark problems and show its advantages in comparison against its non-adaptive versions.},
 author       = {Gladys Castillo},
 school       = {University of Aveiro},
 tags         = {Bayesian network classifiers,Na{{\"}}ve Bayes,adaptive on-line learning systems,concept drift,learning bayesian networks,model selection criteria,prequential framework},
 title        = {Adaptive Learning Algorithms for Bayesian Network Classifiers},
 type         = {Ph.{D}. Thesis},
 year         = {2006},
}

