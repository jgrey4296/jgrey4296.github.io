@inproceedings{GV-CISTI-08,
 author       = {Gonz{\'a}lez-Moreno, Juan Carlos and V{\'a}zquez-L{\'o}pez, Luis},
 title        = {Generaci{\'o}n Autom{\'a}tica del Modelo para Sistemas Multi-Agente},
 year         = {2008},
 booktitle    = {3 Conferencia Ib{\'e}rica de Sistemas y Tecnolog{\i}́as de la Informaci{\'o}n (CISTI 2008)},
 tags         = {__untagged},
 abstract     = {Aunque cada vez m{\'a}s se utilizan los sistemas multiagente, su aplicaci{\'o}n en la metodolog{\i}́a sigue siendo escasa. Las metodolog\{\'a}s de agentes entre las que se encuentran INGENIAS, Prometheus, Tropos, PASSI, Gaia... Una de las carencias que presentan dichas metodolog\'{\' s}, en general, es la cumplimentaci{\'o}n de la fase de an{\'a}lisis de una manera fiable. En cuanto a su realizaci{\'o}n de forma autom{\'a}tica actualmente no existen herramientas. En el proceso de an{\'a}lisis de las necesidades y la adquisici{\'o}n de requisitos, han sido publicados varios trabajos en los {\'u}ltimos a{\~n}os. A pesar de esto, han sido escasas las herramientas que tratan de llevar a la pr{\'a}ctica los resultados obtenidos en los sistemas multiagente. El objetivo de este trabajo es presentar un m{\'e}todo semiautom{\'a}tico para la realizaci{\'o}n de la fase inicial de an{\'a}lisis para INGENIAS a partir de la descripci{\'o}n en lenguaje natural del problema planteado.},
 address      = {Orense, Espa{\~n}a},
 date-added   = {2009-01-03 00:24:27 +0100},
 date-modified= {2009-08-07 09:35:21 +0200},
 isbn         = {84-612-4840-7},
 keywords     = {Itelligent Agents, AOSE, Software Engineering, Natural Language Process, INGENIAS},
 publisher    = {CISTI},
 rating       = {1},
}

