 keywords     = {Hiperlectura; formaci{\'o}n de formadores; Articulaci{\'o}n universidad - escuela media; Educaci{\'o}n y Nuevas Tecnolog{\i}́as; Alfabetizaci{\'o}n Digital.},
 number       = {4},
 url          = {http://www.adie.es/iecom/index.php/IECom/article/view/18},
 volume       = {2},
}

@article{IECom19,
