}

@techreport{AIM-551,
 author       = {McAllester, David A.},
 title        = {An Outlook on Truth Maintenance},
 year         = {1980},
 institution  = {MIT Artificial Intelligence Laboratory},
 tags         = {__untagged},
 abstract     = {Truth maintenance systems have been used in several recent problem solving systems to record justifications for deduced assertions, to track down the assumptions which underlie contradictions when they arise, and to incrementally modify assertional data structures when assumptions are retracted. A TMS algorithm is described here that is substantially different from previous systems. This algorithm performs deduction in traditional propositional logic in such a way that the premise set from which deduction is being done can be easily manipulated. A novel approach is also taken to the role of a TMS in larger deductive systems. In this approach the TMS performs all propositional deduction in a uniform manner while the larger system is responsible for controlling the instantiation of universally quantified formulae and axiom schemas.},
 month        = {aug~6},
 number       = {AIM-551},
 url          = {ftp://publications.ai.mit.edu/ai-publications/500-999/AIM-551.ps; ftp://publications.ai.mit.edu/ai-publications/pdf/AIM-551.pdf},
}

@techreport{AIM-552,
 author       = {Levin, Beth C.},
 title        = {Instrumental With and the Control Relation in English},
 year         = {1979},
 institution  = {MIT Artificial Intelligence Laboratory},
 tags         = {__untagged},
 abstract     = {This paper explores the nature of the underlying representation of a sentence, that representation formulated to make explicit the semantic structure of a sentence as a description of an event. It argues that the typical conception of an underlying representation as a predicate-argument representation, exemplified in systems of case and thematic relations, must be modified. An underlying representation must include semantic relations between noun phrases as well as the predicate-argument relations of noun phrases to a verb. An examination of instrumental with will be used to motivate and justify this revision. In particular, an account of instrumental with requires the introduction of the control relation, a relation between two noun phrases.},
 month        = {nov~6},
 number       = {AIM-552},
 url          = {ftp://publications.ai.mit.edu/ai-publications/500-999/AIM-552.ps; ftp://publications.ai.mit.edu/ai-publications/pdf/AIM-552.pdf},
}

