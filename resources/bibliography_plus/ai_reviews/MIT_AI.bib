 title        = {Suggested Conventions for LISP Time-Sharing System},
 year         = {1963},
 institution  = {MIT Artificial Intelligence Laboratory},
 tags         = {__untagged},
 abstract     = {Below is a list of suggested Conventions and De-bugging aids for LISP time-sharing. Any and all suggestions are encouraged and should be submitted in writing to R. A. Robnett in a hurry.},
 month        = {apr~27},
 number       = {AIM-50},
