 url          = {ftp://publications.ai.mit.edu/ai-publications/1000-1499/AIM-1049.ps; ftp://publications.ai.mit.edu/ai-publications/pdf/AIM-1049.pdf},
}

@techreport{AIM-105,
 author       = {Knight, Tom},
 title        = {Modifications to PDP-6 Teletype Logic},
 year         = {1966},
 institution  = {MIT Artificial Intelligence Laboratory},
 tags         = {__untagged},
