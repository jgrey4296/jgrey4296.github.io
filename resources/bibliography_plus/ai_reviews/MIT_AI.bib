 title        = {Syntactic Closures},
 year         = {1988},
 institution  = {MIT Artificial Intelligence Laboratory},
 tags         = {__untagged},
 abstract     = {In this paper we describe {\i}t syntactic closures. Syntactic closures address the scoping problems that arise when writing macros. We discuss some issues raised by introducing syntactic closures into the macro expansion interface, and we compare syntactic closures with other approaches. Included is a complete implementation.},
 month        = {jun~6},
