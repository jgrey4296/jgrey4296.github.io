@techreport{georgeff:01,
 author       = {Georgeff, Michael P.},
 title        = {The role of artificial intelligence in industrial applications},
 year         = {1989},
 institution  = {Australian Artificial Intelligence Institute},
 tags         = {__untagged},
 abstract     = {This paper looks at the potential for using Artificial Intelligence (AI) in engineering and industrial applications. First, the nature of AI is examined. It is indicated how AI techniques differ from conventional programming techniques and how they extend far beyond those provided by so-called expert systems. Some of the major areas of AI, including natural language understanding, problem solving, and computer vision, are then discussed. It is shown how industry can utilize these technologies by considering some recent work in three areas: real-time control of NASA's space shuttle, reasoning among multiple systems, and just-in-time scheduling. Finally, some of the outstanding problems in AI are described.},
 address      = {Melbourne, Australia},
 month        = {nov},
 number       = {01},
 url          = {ftp://www.aaii.com.au/pub/aaii-technotes/technote01.ps.gz},
}

@techreport{georgeff:03,
 author       = {Georgeff, Michael P. and Ingrand, F{\'e}lix F.},
 title        = {Monitoring and control of spacecraft systems using procedural reasoning},
 year         = {1989},
 institution  = {Australian Artificial Intelligence Institute},
 tags         = {__untagged},
 abstract     = {This paper describes research concerned with automating the monitoring and control of spacecraft systems. In particular, the paper examines the application of SRI's Procedural Reasoning System (PRS) to the handling of malfunctions in the Reaction Control System (RCS) of NASA's space shuttle. Unlike traditional monitoring and control systems, PRS is able to reason about and perform complex tasks in a very flexible and robust manner, somewhat in the manner of a human assistant. Using various RCS malfunctions as examples (including sensor faults, leaking components, multiple alarms, and regulator and jet failures), it is shown how PRS manages to combine both goal-directed reasoning and the ability to react rapidly to unanticipated changes in its environment. In conclusion, some important issues in the design of PRS are reviewed and future enhancements are indicated.},
 address      = {Melbourne, Australia},
 month        = {nov},
 number       = {03},
 url          = {ftp://www.aaii.com.au/pub/aaii-technotes/technote03.ps.gz},
}

@techreport{georgeff:04,
 author       = {Georgeff, Michael P. and Ingrand, F{\'e}lix F.},
 title        = {Decision-making in an embedded reasoning system},
 year         = {1989},
 institution  = {Australian Artificial Intelligence Institute},
 tags         = {__untagged},
 abstract     = {The development of reasoning systems that can reason and plan in a continuously changing environment is emerging as an important area of research in Artificial Intelligence. This paper describes some of the features of a Procedural Reasoning System (PRS) that enables it to operate effectively in such environments. The basic system design is first described and it is shown how this architecture supports both goal-directed reasoning and the ability to react rapidly to unanticipated changes in the environment. The decision-making capabilities of the system are then discussed and it is indicated how the system integrates these components in a manner that takes account of the bounds on both resources and knowledge that typify most real-time operations. The system has been applied to handling malfunctions on the space shuttle, threat assessment, and the control of an autonomous robot.},
 address      = {Melbourne, Australia},
