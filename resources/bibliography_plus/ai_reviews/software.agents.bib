@inproceedings{lingnau:95,
 author       = {Lingnau, Anselm and Drobnik, Oswald and D{\"o}mel, Peter},
 title        = {An HTTP-based Infrastructure for Mobile Agents},
 year         = {1995},
 booktitle    = {Proceedings of the 4th International WWW Conference},
 tags         = {__untagged},
 abstract     = {Mobile agents are an emerging technology attracting interest from the fields of distributed systems, information retrieval, electronic commerce and artificial intelligence. We present an infrastructure for mobile agents based on the Hypertext Transfer Protocol (HTTP) which provides for agent mobility across heterogeneous networks as well as communications among agents. Our infrastructure supports the implementation and interoperation of agents written in various languages and takes advantage of current research in HTTP and the World Wide Web in general.},
 address      = {Boston (MA), USA},
 month        = {dec},
 url          = {http://www.w3.org/pub/Conferences/WWW4/Papers/150/},
}

@inproceedings{lingnau:95b,
 author       = {Lingnau, Anselm and Drobnik, Oswald},
 title        = {An Infrastructure for Mobile Agents},
 subtitle     = {Requirements and Architecture},
 year         = {1995},
 booktitle    = {Proceedings of the 13th DIS Workshop},
 tags         = {__untagged},
 abstract     = {Mobile agents constitute a new approach to the architecture and implementation of distributed systems. A mobile agent is a program with a persistent identity which moves around a network and can communicate with its environment and other agents. Possible applications for mobile agents include network management, information retrieval, distributed simulation, remote device control, active documents and mobile computing. We are developing an infrastructure for the support of mobile agents which is based on a network of `agent servers'. Each of these controls the agents visiting a host, allows for status checks and intervention by the agents' owners and provides a means of communication for the agents under its control. Mobile agents can be written in a number of programming languages such as Tcl and Perl; the agent environment provides runtime support through dedicated libraries that add relocation and communication capabilities to the respective language. Our implementation is based on HTTP (the Hypertext Transfer Protocol) as used on the World-Wide Web; this allows us to leverage off the massive research effort which currently focuses on issues like Web security and enables convenient access to the agent environment via the Web.},
 address      = {Orlando, FL, USA},
 month        = {sep},
 url          = {ftp://ftp.tm.informatik.uni-frankfurt.de/pub/papers/agents/13dis-paper.ps.gz},
}

