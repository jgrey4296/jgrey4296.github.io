@inproceedings{Hutchinson:1995:TAS,
 abstract     = {After reviewing the critical role of speaking and listening (''verbal behavior'') in multiagent coordination and individual ''thinking'' and learning, the paper argues that the full advantages can only be achieved when verbal behavior is learned by agents. Two existing alternatives to this approach are described: programming of agent verbal behavior and training adaptive agents to comply with rules through experience with many examples. Specific advantages are listed for learning as compared with each of these alternatives. The paper describes training adaptive critic agents using methods based on Skinner's analysis of verbal behavior. Using demonstrations which will become available on the Web, the paper describes specific training procedures to teach the agent to imitate, to name objects and properties, to follow instructions, and to memorize verbal advice. Together those skills enable the agent to memorize a rule and correctly follow it the first time the agent sees the condition described in the rule. The early successes reported here, along with the huge practical advantages of the approach if it continues to be successful, encourage us to continue developing and testing agent technology based on this analysis.},
 address      = {Baltimore, MD, USA},
 author       = {William R. Hutchinson},
 booktitle    = {Proceedings of the CIKM'95 Workshop on Intelligent Information Agents},
 editor       = {Tim Finin and James Mayfield},
 notes        = {(Additional Papers)},
 tags         = {},
 title        = {Teaching an Agent to Speak and Listen with Understanding: {W}hy and How?},
 url          = {http://www.cs.umbc.edu/~cikm/iia/submitted/viewing/whutchi.html},
 year         = {1995},
}

@inproceedings{hylton96:knowbot,
 abstract     = {Knowbot Programs are mobile agents intended for use in widely distributed systems like the Internet. We describe our experiences implementing security, process migration, and inter-process communication in a prototype system implemented using the object-oriented programming language Python. This infrastructure supports applications that are composed of multiple, autonomous agents that can migrate to use network resources more efficiently.},
