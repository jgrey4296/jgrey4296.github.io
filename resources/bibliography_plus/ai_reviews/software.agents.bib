@article{Sprite3,
 author       = {Douglis, Fred and Ousterhout, John},
 title        = {Transparent Process Migration},
 subtitle     = {Design Alternatives and the Sprite Implementation},
 year         = {1991},
 journal      = {Software~-- Practice and Experience},
 tags         = {__untagged},
 abstract     = {Most migration uses initial-placement (for performance) then active migration upon eviction. Migration in Sprite almost totally invisible to users: biggest impact is change in workstation loading. In their DS, approx. 30% of all work is done using migration, 86% of that using initial-placement (parallel compiler, pmake). 4 important apects to design: plenty idle hosts; interactive users have exclusive right to the host they are currently using; Sprite uses kernel calls; Sprite already provides network support. Biggest problem = maintaining the state of a process - calls for encapsulation but uses lazy copy (copy-on- reference, logical copying). Centralised file servers. Problems of residual dependencies may not be left on anything other than the home-node (keep things simple). Migration decisions: what to migrate; when (initial-placement or active migration); where; who decides. Good presentation of various strategies already implemented in real systems: from 'rsh' (no policy) to choice of ``pool of processors''. Each Sprite node monitors its own loading and informs a centralised migration server if it thinks it is idle (no input for $>$ 30 seconds) (cf. Mutka & Livny). Average time to perform an initial-placement migration was 330ms (p775). Evictions have been extremely rare. Hosts selected in order of time idle eg 17 hours, 10 hours ... rather than randomly. Great many hosts (66-78% of them) available all of the time. Over 25 day period, over 17,000 processes requested one or more idle hosts and over 86% got all that they wanted - only 2% were unable to obtain any hosts. Overall system performence can be substantial (p782); much use was made of it in Sprite; cost of active migration is high compared with initial-placement. Migration now considered 'indispensible'.},
 month        = {aug},
 number       = {8},
 pages        = {757--785},
 volume       = {21},
}

