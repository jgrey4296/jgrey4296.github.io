@article{bussmann:93,
 author       = {Bussmann, S. and M{\"u}ller, J.},
 title        = {A Communication Architecture for Cooperating Agents},
 year         = {1993},
 journal      = {Computers and Artificial Intelligence},
 tags         = {__untagged},
 abstract     = {Coperation in societies of intelligent units (agents) is almost always established on account of communication between agents. Hence the agents must be equipped with a communication module that allows them to take active part in the negotiation process of the society. Further there have to be a set of keywords and a protocol which define a kind of interlingua. After a detailed motivation, the role of speech acts as a basis for a communication architecture for cooperating agents will be discussed. Viewing them as actions the agents have to perform, the planning aspects will be elaborated. A formal model of communication will then be given which leads, together with the speech act planning, to the communication architecture.},
 number       = {1},
 pages        = {37--53},
 volume       = {12},
}

@incollection{bussmann:93b,
