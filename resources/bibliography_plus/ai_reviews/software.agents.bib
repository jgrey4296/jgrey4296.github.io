 editor       = {S. M. Deen},
 title        = {A Negotiation Framework for Cooperating Agents},
 year         = {1993},
 booktitle    = {1992 Proceedings of the Special Interest Group on Cooperating Knowledge-Based Systems},
 tags         = {__untagged},
 abstract     = {Multi-Agent worlds differ from other domains by the fact that the inhabiting agents have to interact to reach their local goals. Hence, communication (especially negotiation) is the basic action for the agents in order to coordinate their activities. Starting with a general discussion on negotiation aspects and forms of negotiation behaviour a negotiation model will be developed. The negotiation algorithm will be described and the 'Travelling Plan' negotiation domain will demonstrate the application of the introduced concepts. In order to compare the proposed model with other approaches (especially from Distributed Problem Solving) a summary of current negotiation methods will be given.},
 keywords     = {multi-agent systems, negotiation},
 pages        = {1--17},
 publisher    = {Dake Centre, University of Keele},
}

