@inproceedings{Cimatti:1995:MRB,
 abstract     = {As discussed in previous papers, belief contexts are a powerful and appropriate formalism for the representation and implementation of propositional attitudes in a multiagent environment. In this paper we show that a formalization using belief contexts is also elaboration tolerant. That is, it is able to cope with minor changes to input problems without major revisions. Elaboration tolerance is a vital property for building situated agents: it allows for adapting and re-using a previous problem representation in different (but related) situations, rather than building a new representation from scratch. We substantiate our claims by discussing a number of variations to a paradigmatic case study, the Three Wise Men problem.},
 address      = {San Francisco, CA},
 author       = {Alessandro Cimatti and Luciano Serafini},
