@article{abdollahian13_human_devel_dynam_1,
 author       = {Abdollahian, Mark and Yang, Zining and Coan, Travis and Yesilada, Birol},
 title        = {Human Development Dynamics},
 subtitle     = {An Agent Based Simulation of Macro Social Systems and Individual Heterogeneous Evolutionary Games},
 year         = {2013},
 journal      = {Systems Modeling},
 tags         = {ABM,complex_systems,culture,economics,evolution,game_theory},
 abstract     = {Combining a system dynamics and agent based modeling approach, we formalize a simulation framework of the Human Development (HD) perspective. Based on a system of asymmetric, coupled nonlinear equations, we first capture the core logic of HD theory, empirically validated from World Values Survey (WVS) data. Using the framework of evolutionary game theory, second we fuse these endogenously derived individual socio-economic attribute changes with Prisoner's Dilemma in an agent based framework to model the interactive political-cultural effects of heterogeneous, spatial intra-societal economic transactions. We explore the model's behavioral dynamics via simulation methods to identify paths and pitfalls towards economic development, cultural plasticity, and social change behavior. Our preliminary results suggest strong nonlinear path dependence in three areas: development processes, evolutionary intra-societal transactions and development trajectories, with significant implications for anticipating and managing positive development outcomes.},
 doi          = {10.1186/2194-3206-1-18},
 file         = {2013/Abdollahian/Abdollahian_2013_Human_Development_Dynamics.pdf},
 isbn         = {9783319055787},
 issn         = {2194-3206},
 number       = {1},
 pages        = {1--14},
 url          = {http://www.casmodeling.com/content/1/1/18/},
 volume       = {1},
}

@inproceedings{abeele13_more,
 author       = {Abeele, Vero Vanden and Schutter, Bob De and Gajadhar, Brian and Johnson, Daniel and Geurts, Luc},
 title        = {More naturalness, less control},
 subtitle     = {The effect of natural mapping on the co-located player experience},
 year         = {2013},
 booktitle    = {foundations of digital games (fdg)},
 tags         = {HCI,games,gender,hardware,space},
 abstract     = {In the past years, there has been a surge in game controllers that allow players to play in a more physical, more natural way. In this paper we present an experimental study of the effect of gaming using these naturally mapped controllers on the player experience in a social setting. Results support the hypothesis that more naturally mapped controllers augment spatial presence. Furthermore, the results suggest that gaming with more naturally mapped controllers augment social presence for female players, but not for male players. However, gaming via naturally mapped controllers decreases perceived control and actual performance. Hence, users with high performance expectations might not benefit from gaming via naturally mapped controllers.},
 file         = {2013/Abeele/Abeele_2013_More_naturalness_less_control.pdf},
 number       = {September},
 pages        = {174--182},
 url          = {http://eprints.qut.edu.au/63603/},
}

