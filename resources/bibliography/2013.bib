 journal      = {Economics and Philosophy},
 number       = {02},
 pages        = {175--198},
 tags         = {belief,bias,evasion,game_theory,manipulation,norms,social},
 title        = {Norm Manipulation, Norm Evasion: Experimental Evidence},
 url          = {http://www.journals.cambridge.org/abstract_S0266267113000187},
 volume       = {29},
