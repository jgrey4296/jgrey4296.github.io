 issn         = {15504298},
 journal      = {Journal of GLBT Family Studies},
 number       = {5},
 pages        = {425--445},
 publisher    = {Taylor \& Francis},
 tags         = {LGBTQ,children,eugenics,gender,reproductive_rights,sexuality,sterilisation,the_state,trans},
 title        = {Reproducing Eugenics, Reproducing While Trans: the State Sterilization of Trans People},
