 number       = {11},
 pages        = {201715374},
 pmid         = {29487213},
 tags         = {authoring,contribution,division_of_labour,responsibility,taxonomy,writing},
 title        = {Transparency in Authors' Contributions and Responsibilities To Promote Integrity in Scientific Publication},
