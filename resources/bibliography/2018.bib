 journal      = {Perspectives on Politics},
 number       = {4},
 pages        = {969--989},
 tags         = {dystopia,fiction,politics,radicalisation,sci_fi,speculative_fiction,violence},
 title        = {It's the End of the World and They Know It: How Dystopian Fiction Shapes Political Attitudes},
 volume       = {16},
 year         = {2018},
}



