 pages        = {1--14},
 tags         = {Egison,data_structure,kleene_star,language,non_linear,pattern_matching,programming,semantics,syntax},
 title        = {Loop Patterns: Extension of Kleene Star Operator for More Expressive Pattern Matching Against Arbitrary Data Structures},
 url          = {http://arxiv.org/abs/1809.03252},
 year         = {2018},
}



