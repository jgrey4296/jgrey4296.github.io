 file         = {2011/Kalnikaite/Kalnikaite_2011_How_To_Nudge_in_Situ_Designing_Lambent_.pdf},
 isbn         = {9781450306300},
 pages        = {11--20},
 tags         = {HCI,decision_making,design,nudge,persuasive_technology,shopping,supermarket,tangible},
 title        = {How To Nudge in Situ: Designing Lambent Devices To Deliver Salient Information in Supermarkets},
 url          = {http://dl.acm.org/citation.cfm?id=2030112.2030115},
 year         = {2011},
}



