 issn = {15487458},
 journal = {Visual Anthropology Review},
 number = {1},
 pages = {18--27},
 tags = {israel,art,palestine,landscape,politics,photography,anthropology},
 title = {Framing Visual Politics: Photography of the Wall in Palestine},
 volume = {33},
 year = {2017}
}

