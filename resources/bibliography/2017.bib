 file = {~/Mega/pdflibrary/2017/Mensh, Kording/Mensh, Kording_2017_Ten simple rules for structuring papers.pdf},
 hashes = {f02a2c14c630369f2058403a448f94e7aaeafb3c1cc681c9d6862812590b7b6c},
 isbn = {0894-0282},
 issn = {1553-7358},
 journal = {PLOS Computational Biology},
 month = {sep},
 number = {9},
 pages = {e1005619},
 pmid = {22352717},
 tags = {guidelines,writing,structure},
 title = {Ten simple rules for structuring papers},
