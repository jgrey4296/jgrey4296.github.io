 journal = {Leibniz International Proceedings in Informatics, LIPIcs},
 pages = {211--2127},
 tags = {programming,pivot_tables,type_checking,literacy,object_calculus,exploration,type_providers,DSL,language},
 title = {Data exploration through dot-driven development},
 url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85037807982&doi=10.4230%2FLIPIcs.ECOOP.2017.21&partnerID=40&md5=f24bb6befcf2c7840c5c0769e802b251},
