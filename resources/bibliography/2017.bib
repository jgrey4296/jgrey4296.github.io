 hashes = {80c19ec70c7235656864c6ec89d6ba216ead984fc61536a25712af97cfb93cc0},
 isbn = {9781450355254},
 issn = {1439-7609},
 pages = {43--54},
 pmid = {24252038},
 tags = {taxonomy,software_engineering,analysis,mistake,language,grammar},
 title = {Towards a taxonomy of grammar smells},
 url = {http://dl.acm.org/citation.cfm?doid=3136014.3136035},
 year = {2017}
