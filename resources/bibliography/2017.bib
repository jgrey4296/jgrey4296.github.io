 hashes = {2d897c982b29a60192aecc0aaa5db66a2a3a6080c45e371fdb022bb6f1c09192},
 isbn = {9781450346603},
 issn = {03621340},
 journal = {ACM SIGPLAN Notices},
 number = {1},
 pages = {804--817},
 tags = {refinement_types,gradual_typing,semantics,type_checking,types,uncertainty,algorithm,refinements},
 title = {Sums of uncertainty: refinements go gradual},
 url = {http://dl.acm.org/citation.cfm?doid=3093333.3009865},
