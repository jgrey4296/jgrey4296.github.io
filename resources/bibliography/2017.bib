 issn = {23304847},
 journal = {Economic Anthropology},
 number = {1},
 pages = {94--106},
 pmid = {28413810},
 tags = {temporality,islam,preacher_economy,field,time,preaching,social_media,social_capital,pierre_bourdieu,anthropology,network},
 title = {A subtle economy of time: Social media and the transformation of Indonesia's Islamic preacher economy},
