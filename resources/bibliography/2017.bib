 issn = {19493045},
 journal = {IEEE Transactions on Affective Computing},
 tags = {MorpheuS,search,affect,metrics,music,patterns,variable_neighbourhood,visualisation,constraints,procedural_content},
 title = {MorpheuS: generating structured music with constrained patterns and tension},
 year = {2017}
