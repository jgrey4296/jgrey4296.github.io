@inproceedings{colledanchise14_how_behav_trees_modul_robus_,
 abstract     = {Behavior Trees (BTs) have become a popular framework for designing controllers of in-game opponents in the computer gaming industry. In this paper, we formalize and analyze the reasons behind the success of the BTs using standard tools of robot control theory, focusing on how properties such as robustness and safety are addressed in a modular way. In particular, we show how these key properties can be traced back to the ideas of subsumption and sequential compositions of robot behaviors. Thus BTs can be seen as a recent addition to a long research effort towards increasing modularity, robustness and safety of robot control software. To illustrate the use of BTs, we provide a set of solutions to example problems.},
 author       = {Colledanchise, Michele and {\"O}gren, Petter},
 booktitle    = {IEEE International Conference on Intelligent Robots and Systems},
 doi          = {10.1109/IROS.2014.6942752},
 file         = {2014/Colledanchise/Colledanchise_2014_How_Behavior_Trees_Modularize_Robustness.pdf},
 isbn         = {9781479969340},
 issn         = {21530866},
 pages        = {1482--1488},
 tags         = {FSMs,JADE,ROS,behaviour_tree,design,modularity,robotics,robustness,safety},
 title        = {How Behavior Trees Modularize Robustness and Safety in Hybrid Systems},
 year         = {2014},
}



@inproceedings{colledanchise14_perfor_analy_stoch_behav_trees_,
 abstract     = {This paper presents a mathematical framework for performance analysis of Behavior Trees (BTs). BTs are a recent alternative to Finite State Machines (FSMs), for doing modular task switching in robot control architectures. By encoding the switching logic in a tree structure, instead of distributing it in the states of a FSM, modularity and reusability are improved. In this paper, we compute performance measures, such as success/failure probabilities and execution times, for plans encoded and executed by BTs. To do this, we first introduce Stochastic Behavior Trees (SBT), where we assume that the probabilistic performance measures of the basic action controllers are given. We then show how Discrete Time Markov Chains (DTMC) can be used to aggregate these measures from one level of the tree to the next. The recursive structure of the tree then enables us to step by step propagate such estimates from the leaves (basic action controllers) to the root (complete task execution). Finally, we verify our analytical results using massive Monte Carlo simulations, and provide an illustrative example of the results for a complex robotic task.},
 author       = {Colledanchise, Michele and Marzinotto, Alejandro and Ogren, Petter},
 booktitle    = {Proceedings - IEEE International Conference on Robotics and Automation},
 doi          = {10.1109/ICRA.2014.6907328},
 file         = {2014/Colledanchise/Colledanchise_2014_Performance_Analysis_of_Stochastic_Behav.pdf},
 isbn         = {978-1-4799-3685-4},
 issn         = {10504729},
 pages        = {3265--3272},
 tags         = {DTMC,__printed,ai,behaviour_tree,efficiency,markov,metrics,performance,probability,stochastic},
 title        = {Performance Analysis of Stochastic Behavior Trees},
 year         = {2014},
}



