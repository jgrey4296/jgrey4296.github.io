 hashes       = {3af5e0b12c6a34f3805f5103210a8088ef44811ce0a9beab03aad8c10ebc9ebf},
 isbn         = {9781467351157},
 pages        = {384--389},
 tags         = {SMart,algorithm,art,bharata_natyam,choreography,dance,evolution,procedural_content},
 title        = {Art To Smart: an Evolutionary Computational Model for Bharatanatyam Choreography},
 year         = {2012}
}

