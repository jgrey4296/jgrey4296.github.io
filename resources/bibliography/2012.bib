 file         = {~/Mega/pdflibrary/2012/Prada, Raimundo, Dimas/Prada, Raimundo, Dimas_2012_The role of social identity, rationality and anticipation in believable agents.pdf},
 hashes       = {c5caaf2170c2853d0fe937acf9c793399cb05c98e12f0427e10078bca4c17b50},
 tags         = {HCI,anticipation,behaviour,design,identity,rationality,social},
 title        = {The Role of Social Identity, Rationality and Anticipation in Believable Agents},
