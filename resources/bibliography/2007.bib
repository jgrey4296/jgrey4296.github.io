 publisher    = {MIT Press},
 tags         = {complex_networks,computation,games,procedural_criticism,procedural_subjectivity,simulation,structuralism,unit_operations},
 title        = {Unit Operations: An Approach to Videogame Criticism},
 volume       = {55},
