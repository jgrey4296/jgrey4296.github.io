@article{boella07_ontol_proper_social_roles_multi_agent_system_15,
 abstract     = {In this paper we address the problem of defining social roles in multi-agent systems. Social roles provide the basic structure of social institutions and organizations. We start from the properties attributed to roles both in the multi-agent systems and the Object Oriented community, and we use them in an ontological analysis of the notion of social role. We identify three main properties of social roles. First, they are definitionally dependent on the institution they belong to, i.e. the definition of a role is given inside the definition of the institution. Second, they attribute powers to the agents playing them, like creating commitments for the institutions and the other roles. Third, they allow roles to play roles, in the same way as agents do. Using Input/Output logics, we propose a formalization of roles in multi-agent systems satisfying the three properties we identified.In this paper we address the problem of defining social roles in multi-agent systems. Social roles provide the basic structure of social institutions and organizations. We start from the properties attributed to roles both in the multi-agent systems and the Object Oriented community, and we use them in an ontological analysis of the notion of social role. We identify three main properties of social roles. First, they are definitionally dependent on the institution they belong to, i.e. the definition of a role is given inside the definition of the institution. Second, they attribute powers to the agents playing them, like creating commitments for the institutions and the other roles. Third, they allow roles to play roles, in the same way as agents do. Using Input/Output logics, we propose a formalization of roles in multi-agent systems satisfying the three properties we identified.},
 author       = {Boella, Guido and Van Der Torre, Leendert},
 doi          = {10.1007/s10506-007-9030-8},
 file         = {2007/Boella/Boella_2007_The_Ontological_Properties_of_Social_Rol.pdf},
 isbn         = {09248463},
 issn         = {09248463},
 journal      = {Artificial Intelligence and Law},
 number       = {3},
 pages        = {201--221},
 pmid         = {216173032},
 tags         = {MAS,input_output_logics,institution,logic,ontology,roles,social,taxonomy},
 title        = {The Ontological Properties of Social Roles in Multi-Agent Systems: Definitional Dependence, Powers and Roles Playing Roles},
