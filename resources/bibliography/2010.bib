 isbn         = {9781605587998},
 pages        = {951},
 publisher    = {ACM Press},
 tags         = {ATOMate,HCI,PIM,UI,design,information_retrieval,prototype},
 title        = {Atomate It! End-User Context-Sensitive Automation Using Heterogeneous Information Sources on the Web},
 url          = {http://portal.acm.org/citation.cfm?doid=1772690.1772787},
 year         = {2010},
