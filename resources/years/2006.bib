 title = {The model human processor and the older adult: Validation and error extension to GOMS in a mobile phone task},
 url = {http://ezproxy.net.ucf.edu/login?url=http://search.proquest.com/docview/305327970?accountid=10003%5Cnhttp://sfx.fcla.edu/ucf?url_ver=Z39.88-2004&rft_val_fmt=info:ofi/fmt:kev:mtx:dissertation&genre=dissertations+&+theses&sid=ProQ:ProQuest+Dissertations+&+T},
