@article{Caramazza2006,
 abstract = {Cognitive neuropsychology began in the second half of the nineteenth century. The cognitive neuropsychologists were also cognitive neuroscientists: They were interested not only in the functional architecture of cognition, but also in how the components of such an architecture were localized in the brain. Cognitive neuroscience is the study of those neural systems of the brain that subserve cognition, and so it is a branch of neuroscience. The view of cognitive neuropsychology as a branch of cognitive psychology and as distinct from cognitive neuroscience is widely accepted. Characteristic of cognitive neuropsychology is that it studies symptoms rather than syndromes and carries out single case studies rather than group studies. Some patients with agrammatic sentence constructions are impaired at the use of function words but not at the use of affixes, whereas others show the opposite dissociation. This is one reason why cognitive neuropsychologists study symptoms, not syndromes. The number of possible different shapes for snowflakes is so large that the likelihood of obtaining two snowflakes with the same morphology is quite small, which makes the prospect of group studies of snowflake morphology an impractical one. The papers in this volume show that computational modelling is rapidly becoming important in cognitive neuropsychology. As already noted, cognitive neuropsychology can be considered a branch of cognitive psychology where subjects' performance is used to inform theories of normal cognition. However, as also already noted, there is increasing interest in relating cognitive neuropsychological investigations to developments in cognitive neuroscience. (PsycINFO Database Record (c) 2006 APA},
 author = {Caramazza, Alfonso and Coltheart, Max},
 doi = {10.1080/02643290500443250},
 file = {~/Mega/pdflibrary/2006/Caramazza, Coltheart/Caramazza, Coltheart_2006_Cognitive neuropsychology twenty years on.pdf},
 hashes = {0b85bdd15fdb93ff5ff3b02912913c377bf6367e729320df34b2eea1d574cc51},
 isbn = {0264-3294},
