 issn = {09688080},
 journal = {Reproductive Health Matters},
 number = {27},
 pages = {181--190},
 tags = {abortion, reproductive_rights, prague, mental_health, psychology, pregnancy, czech_republic},
 title = {Born Unwanted, 35 Years Later: The Prague Study},
 volume = {14},
 year = {2006}
}

