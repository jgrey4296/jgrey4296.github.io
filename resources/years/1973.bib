@book{Berne1973,
 abstract = {Do you realise you, and all the people you know, play games? All the time? Sexual games, marital games, complex games that you're not even aware of as you go about your usual life? You might play games like 'Alcoholic' or 'The Frigid Woman' at weekends, or perhaps 'Ain't it awful' or 'Kick me' while you're at work. First published in the 1960s and recognized as a classic work of its kind by professionals, the bestselling "Games People Play" is also an accessible and fascinating read. It is a wise, original, witty and very sensible analysis of the games we play in order to live with one another - and with ourselves.},
 author = {Berne, Eric},
 doi = {0140027688},
 file = {~/Mega/pdflibrary/1973/Berne/Berne_1973_Games People Play The Psychology of Human Relationships.pdf},
