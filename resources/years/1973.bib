 isbn = {1939-1315},
 issn = {0022-3514},
 journal = {Journal of Personality and Social Psychology},
 number = {3},
 pages = {446--456},
 pmid = {3046},
 tags = {paper,social,phenomena,theory,experimentation,psychology,rule},
 title = {The yin and yang of progress in social psychology: Seven koan.},
 url = {http://content.apa.org/journals/psp/26/3/446 http://psycnet.apa.org/journals/psp/26/3/446/},
