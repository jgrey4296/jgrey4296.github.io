 pages = {424--35},
 pmid = {21199846},
 tags = {culture, traits, social_learning, learning, population, distribution, diversity},
 title = {On the number of independent cultural traits carried by individuals and populations.},
 url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3013478&tool=pmcentrez&rendertype=abstract},
 volume = {366},
