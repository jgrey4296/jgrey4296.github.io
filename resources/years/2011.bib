 journal = {Computers in Human Behavior},
 number = {1},
 pages = {268--275},
 publisher = {Elsevier Ltd},
 tags = {MMORPGs, taboos, games, online, multiplayer, psychology, phenomenology, culture},
 title = {What i won't do in pixels: Examining the limits of taboo violation in MMORPGs},
 url = {http://dx.doi.org/10.1016/j.chb.2010.08.004},
 volume = {27},
 year = {2011}
}

