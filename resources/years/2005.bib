 pages = {278--291},
 tags = {MAS, character, storytelling, believability, transforms, Fabulist, QUEST, question_answering, planning},
 title = {An objective character believability evaluation procedure for multi-agent story generation systems},
 volume = {3661 LNAI},
 year = {2005}
