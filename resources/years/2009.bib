  pmid = {15061508},
  primaryclass = {doi:10.1038},
  tags = {activity,collective_intelligence,crowds,tasks,organisation},
  title = {Harnessing crowds: Mapping the genome of collective intelligence},
  volume = {1},
  year = {2009}
  }

