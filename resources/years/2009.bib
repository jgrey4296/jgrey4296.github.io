  isbn = {9781605582467},
  pages = {463},
  tags = {NLP,LIWC,blog,psychology,autism,observation,communication,blogging,HCI},
  title = {Autism online: A Comparison of Word Usage in Bloggers With and Without Autism Spectrum Disorders},
  url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84892469747&partnerID=tZOtx3y1%5Cnhttp://dl.acm.org/citation.cfm?doid=1518701.1518775},
