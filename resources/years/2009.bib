  tags = {typology,taxonomy,task,interruption,observation,ethnology,interleaving},
  title = {Self-interruption on the computer: A Typology of Discretionary Task Interleaving},
  url = {http://dl.acm.org/citation.cfm?doid=1518701.1518979},
  year = {2009}
