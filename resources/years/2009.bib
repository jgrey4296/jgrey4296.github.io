  title = {From abstraction to reality: Integrating drama management into a playable game experience},
  url = {http://www.aaai.org/Papers/Symposia/Spring/2009/SS-09-06/SS09-06-018.pdf http://www.aaai.org/Papers/Symposia/Spring/2009/SS-09-06/SS09-06-018.pdf%5Cnhttp://scholar.google.com/scholar?hl=en&btnG=Search&q=intitle:From+Abstraction+to+Reality+:+Integrating+Dr},
