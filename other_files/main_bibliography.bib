 journal = {BMC Biology},
 number = {1},
 pages = {7},
 pmid = {28173793},
 publisher = {BMC Biology},
 tags = {Sex-differential expression,sexual dimorphism,Sex-differential selec,sex-differential expression,theory,game,meta,phenomena,sex-differential selection},
 title = {The landscape of sex-differential transcriptome and its consequent selection in human adults},
