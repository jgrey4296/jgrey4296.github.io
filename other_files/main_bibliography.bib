 isbn = {10468188},
 issn = {10468188},
 journal = {ACM Transactions on Information Systems},
 number = {October},
 pages = {431--456},
 pmid = {6067643},
 tags = {theory,taxonomy,institution,rule},
 title = {Genre Taxonomy : A Knowledge Repository of Communicative Actions},
