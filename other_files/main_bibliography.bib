 issn = {1095-9572},
 month = {oct},
 number = {4},
 pages = {1285--94},
 pmid = {18585466},
 tags = {Raclopride: pharmacokinetics,Receptors,Male,Pyrrolidines: pharmacokinetics,Dopamine D2,Pyrrolidines,Image Interpretation,Radiopharmaceuticals,Carbon Radioisotopes,Brain: drug effects,Brain,Carbon Radioisotopes: diagnostic use,Dopamine D2: metabolism,Raclopride,Salicylamides,Salicylamides: diagnostic use,Brain: radionuclide imaging,Computer-Assisted,Adult,agent,Tissue Distribution,Salicylamides: pharmacokinetics,Radiopharmaceuticals: pharmacokinetics,Brain: metabolism,Pyrrolidines: diagnostic use,Antipsychotic Agents: administration & dosage,Computer-Assisted: methods,Antipsychotic Agents,Raclopride: diagnostic use,Carbon Radioisotopes: pharmacokinetics,Sensitivity and Specificity,Reproducibility of Results,Humans,Radiopharmaceuticals: diagnostic use,Papers},
