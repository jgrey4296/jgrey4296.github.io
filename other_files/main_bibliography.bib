 journal = {Time},
 number = {11-06-024},
 pages = {1--55},
 tags = {agent,MAS,authority,theory,rule,game,phenomena,Papers,publicationless,norm},
 title = {How to Do Agent-Based Simulations in the Future : From Modeling Social Mechanisms to Emergent Phenomena and Interactive Systems Design Why Develop and Use Agent-Based Models ?},
