 hashes = {7a87a94af6c20155049c1aaf6b75e6b91f4e1db222c39ee5f08eff047d9a7ed8},
 isbn = {1071-5819, Print},
 issn = {10715819},
 journal = {International Journal of Human-Computer Studies},
 month = {jul},
