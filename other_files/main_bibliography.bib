 number = {2},
 pages = {209--240},
 tags = {agent,MAS,theory,institution,rule,game,mateas,Papers},
 title = {Interaction, narrative, and drama: Creating an adaptive interactive narrative using performance arts theories},
 url = {http://www.jbe-platform.com/content/journals/10.1075/is.8.2.03eln http://openurl.ingenta.com/content/xref?genre=article&issn=1572-0373&volume=8&issue=2&spage=209},
 volume = {8},
