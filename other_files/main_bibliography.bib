 number = {3},
 pages = {329--346},
 tags = {The Fappening,Reddit,platform politics,gender,online communities,toxic technocultures,Algorithms,online harassment,design,Gamergate},
 title = {#Gamergate and The Fappening: How Reddit's algorithm, governance, and culture support toxic technocultures},
