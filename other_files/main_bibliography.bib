 journal = {Philosophical Transactions of the Royal Society B: Biological Sciences},
 number = {1753},
 pmid = {29967303},
 tags = {Communication,Spatial boundaries,agent,Collective intelligence,theory,Transparency,rule,Collaboration,Interaction},
 title = {The impact of the ‘open' workspace on human collaboration},
 volume = {373},
 year = {2018}
