@article{Klockner2015,
 abstract = {A robust autopilot design methodology using linear parameter varying transformations is presented and applied to a high-agile surface launched air defence missile, which is currently developed by Diehl-BGT-Defence. The lateral dynamics of the tail/thrust vector controlled missile are modelled as a second-order quasi-linear parameter varying (LPV) system. The incidence angle is used as exogenous variable, which is assumed to be estimated during missile flight. Decoupled lateral dynamics are assumed because of the application of a bank-to-turn manoeuvre plane angle control approach. Lateral single channel flight controllers are designed via H∞ -optimal control and $μ$-synthesis with the LPV lateral dynamics, which are extended by uncertain models of control actuating system, time-delay and body bending model. The flight controllers for lateral dynamics are designed at a number of operating points described by the LPV model over the MACH flight envelope. The controllers are implemented using a gain scheduling approach, where an altitude dependent gain loss in the control loop is compensated with the inverse normalised air density. The flight controllers were implemented in the nonlinear simulation environment and tested in extreme flight manoeuvres. All flight controllers showed good damping and acceleration tracking performance and were stable during nonlinear simulations.},
 archiveprefix = {arXiv},
 arxivid = {arXiv:1011.1669v3},
 author = {Klockner, Andreas},
 doi = {10.1007/978-3-319-17518-8},
 eprint = {arXiv:1011.1669v3},
 file = {/Volumes/DOCUMENTS/MEGA/Mendeley/2015/Klockner/Klockner_2015_Behavior Trees with Stateful Tasks.pdf},
 hashes = {f36aaf543bfc7f45f13eee0a63a602d120227b60d1d309d99f4423731413d2bd},
 isbn = {978-3-319-17517-1},
 issn = {1098-6596},
 pmid = {25246403},
 tags = {state machines,mission management,behavior trees,arxiv,game},
 title = {Behavior Trees with Stateful Tasks},
 url = {http://link.springer.com/10.1007/978-3-319-17518-8},
 year = {2015}
}

@article{Klonick2018,
 abstract = {The hybrid field of artificial intelligence (AI) is still an 'open'$\backslash$nintellectual frontier which psychologists, together with computer$\backslash$nscientists, anthropologists, cognitive biologists, linguists, physicists$\backslash$nand philosophers, among others, can explore on a more or less equal$\backslash$nfooting (Adams-Webber, 1993). Their ultimate goal is to design an$\backslash$nabstract machine whose linguistic behaviour is equivalent to that$\backslash$nof humans (e.g. Turing, 1950). That objective presupposes that the$\backslash$nconceptual bases of all of our linguistic behaviour are amenable$\backslash$nto formalization. Others subscribe to the more modest agenda of using$\backslash$ncomputational procedures to model only some aspects of human cognition,$\backslash$nfor example, 'expertise' in chess or medical diagnosis (see Chapter$\backslash$n8, pp. 87-94). Proponents of both approaches have developed simulation$\backslash$nprograms which are models of human cognitive processes in the form$\backslash$nof computer software. Such models are essentially theories. The main$\backslash$nidea is that the linguistic behaviour of the program should closely$\backslash$napproximate that of humans (Simon, 1995). The detailed internal functions$\backslash$nof the model are viewed as a potential theoretical explanation of$\backslash$nthe corresponding human performance when the input-output behaviour$\backslash$nof the computer running the program closely resembles that of humans.$\backslash$nThese computational models can be specified fully in mathematical$\backslash$nterms and are unprecedented in the field of psychology with respect$\backslash$nto their level of precision and internal consistency. George Alexander$\backslash$nKelly's Range Corollary applies to an important problem in the theory$\backslash$nof knowledge which has extensive implications throughout the field$\backslash$nof AI. That is, the frame problem (also called the 'temporal projection$\backslash$nproblem'). Perhaps the most interesting facet of the frame problem$\backslash$nfrom a psychological perspective is that humans, in contrast to computer$\backslash$nprograms, rarely suffer from it. The Range Corollary also entails$\backslash$nthe possibility of differentiation of function, not only among constructs,$\backslash$nbut also systems and subsystems of interrelated constructs in terms$\backslash$nof their combined ranges of convenience (Adams-Webber, 1996). By$\backslash$nextension, the more functionally differentiated any system or subsystem$\backslash$nof constructs, the wider the potential range of relevance ('frame')$\backslash$nof hypotheses that can be derived within that system. Kelly's (1969J,$\backslash$npp. 119-120) 'minimax principle' implies that an ideal cognitive$\backslash$nsystem, either human or artificial, would allow the maximum number$\backslash$nof differential predictions concerning future events with the minimal$\backslash$nnumber of constructs. (PsycINFO Database Record (c) 2005 APA, all$\backslash$nrights reserved) (from the create)},
 archiveprefix = {arXiv},
 arxivid = {astro-ph/0208106},
 author = {Klonick, Kate},
 doi = {10.1146/annurev.astro.40.060401.093840},
 eprint = {0208106},
 file = {/Volumes/DOCUMENTS/MEGA/Mendeley/2018/Klonick/Klonick_2018_The new governors The people, rules, and processes governing online speech.pdf},
 hashes = {a856efa37d75ebf5cccef5f1f574d8e77b75349bbadde2923043beea3cd9c198},
 isbn = {0-470-01323-0},
 issn = {0017811X},
 journal = {Harvard Law Review},
 number = {6},
 pages = {1599--1670},
 primaryclass = {astro-ph},
 tags = {agent,theory,rule,institution,taxonomy,calculus,norm},
 title = {The new governors: The people, rules, and processes governing online speech},
 volume = {131},
 year = {2018}
}

@article{Kluttz2016,
 abstract = {This Handbook provides the hidden common threads that tie sociological inquiry together and, featuring eminent scholars, it separates itself from its predecessors in substance and organization. Rather than rehashing old debates or longingly gazing at the past, this book presents sociologists with new ways of conceptualizing the organization and presentation of sociological theory. At the heart of this Handbookℓ́ℓs vision is the twin goals of making theory a viable enterprise by reconceptualizing how we teach theory and keeping theory closely tied to its empirical applications. Three strategies are offered: (1) Elucidating how classic issues like integration or interaction are interrogated today; (2) Presenting a coherent vision of the social levels of reality that theorists work on such as communities, groups, and the self as well as how the coherence of these levels speaks to the macro-micro link; and, (3) Theorizing the social world rather than celebrating theorists or theories; that is, one can look at how theory is used holistically to understand the constraints the social world places on our lived experience or the dynamics of social change. Hence, in the second decade of the 21st century, it has become clear that sociology is at a crossroads as the number of theorists and amount of theory available is increasingly unmanageable and unknowable by the vast majority of professionals and students. As such, this Handbook of Contemporary Sociological Theory presents the novice and the expert with the a roadmap for traversing this crossroad and building a more coherent, robust, and cumulative sociology.},
 author = {Kluttz, Daniel N and Fligstein, Neil},
 doi = {10.1007/978-3-319-32250-6},
 file = {/Volumes/DOCUMENTS/MEGA/Mendeley/2016/Kluttz, Fligstein/Kluttz, Fligstein_2016_Varieties of Sociological Field Theory.pdf},
 hashes = {dc96ddf80f63facfd6f8ffb9bbcda4f803ec2c70690600370fce869917ec45bb},
 isbn = {978-3-319-61601-8},
 tags = {agent,institution,rule,theory,phenomena,husserl,bourdieu,ritual,sanction,authority},
 title = {Varieties of Sociological Field Theory},
 url = {http://link.springer.com/10.1007/978-3-319-32250-6},
 year = {2016}
}

@article{Knight1989,
 abstract = {Two experiments investigate the effects of language comprehension on affordances. Participants read a sentence composed by either an observation or an action verb (Look at/Grasp) followed by an object name. They had to decide whether the visual object following the sentence was the same as the one mentioned in the sentence. Objects graspable with either a precision or a power grip were presented in an orientation affording action (canonical) or not. Action sentences were faster than observation sentences, and power grip objects were faster than precision grip objects. Moreover, faster RTs were obtained when orientation afforded action. Results indicate that the simulation activated during language comprehension leads to the formation of a "motor prototype" of the object. This motor prototype encodes information on temporary/canonical and stable affordances (e.g., orientation, size), which can be possibly referred to different cognitive and neural systems (dorsal, ventral systems). (C) 2008 Elsevier B.V. All rights reserved.},
