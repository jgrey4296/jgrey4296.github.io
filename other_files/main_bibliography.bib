 hashes = {fa007acd1da85d38bd9ef15dff39776b4eb8c198830b2fe798ca8f8c8e0e7f77},
 issn = {15563537},
 journal = {Anthropology of Consciousness},
 number = {1},
 pages = {31--68},
 tags = {brain,complexity of consciousness,institution,rule,theory,meta,phenomena,husserl,conceptual systems theory,ritual,ways of knowing},
 title = {Conceptual Systems Theory: A Neglected Perspective for the Anthropology of Consciousness},
