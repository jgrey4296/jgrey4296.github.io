@article{Furnari2014a,
 abstract = {I develop a model linking specific microinteraction dynamics between$\backslash$nmembers of different institutional fields and the genesis of new$\backslash$npractices. The model centers on the concept of interstitial spaces that$\backslash$nis, small-scale settings where individuals from different fields$\backslash$ninteract occasionally and informally around common activities to which$\backslash$nthey devote limited time (e.g., hobbyist clubs, hangouts, workshops,$\backslash$nmeet-ups). I argue that the features of interstitial spaces (e.g., their$\backslash$ninstitutional diversity and their occasional and informal nature)$\backslash$nfacilitate the individuals interacting in these settings to temporarily$\backslash$nbreak free from existing institutions and experiment collectively with$\backslash$nnew activities and ideas. However, these very same features hinder the$\backslash$nconstitution of such new activities and ideas into new practices. I$\backslash$nidentify two microlevel conditions that enable the new activities and$\backslash$nideas developed in interstitial spaces to be constituted into new$\backslash$npractices: the emergence of successful interaction rituals, and the$\backslash$npresence of catalysts sustaining others' interactions and assisting the$\backslash$nconstruction of shared meanings.},
 author = {Furnari, Santi},
 doi = {10.5465/amr.2012.0045},
 file = {/Volumes/DOCUMENTS/MEGA/Mendeley/2014/Furnari/Furnari_2014_Interstitial spaces Microinteraction settings and the genesis of new practices between institutional fields.pdf},
