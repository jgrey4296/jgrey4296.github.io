 pmid = {20363178},
 publisher = {Elsevier Ltd},
 tags = {Brain,agent,theory,Humans,Animals,Brain: anatomy & histology,phenomena,Cognition: physiology,Papers,Cognition},
 title = {Towards a bottom-up perspective on animal and human cognition},
 url = {http://dx.doi.org/10.1016/j.tics.2010.03.003 http://www.ncbi.nlm.nih.gov/pubmed/20363178},
