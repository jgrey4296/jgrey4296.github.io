@article{Artikis2012,
 abstract = {Multi-Agent Systems (MAS) where the agents are developed by different$\backslash$nparties and serve different interests, are often classified as `open'.$\backslash$nThe specification of open MAS is largely seen as a design-time activity.$\backslash$nMoreover, there is no support for run-time specification modification.$\backslash$nDue to environmental, social or other conditions, however, it is often$\backslash$nrequired to revise the MAS specification during its execution. To$\backslash$naddress this requirement, we present an infrastructure for `dynamic'$\backslash$nopen MAS specifications, that is, specifications that may be modified at$\backslash$nrun-time by the agents. We adopt a bird's eye view of an open MAS, as$\backslash$nopposed to an agent's own perspective whereby it reasons about how it$\backslash$nshould act. The infrastructure consists of well-defined procedures for$\backslash$nproposing a modification of the `rules of the game', as well as$\backslash$ndecision-making over and enactment of proposed modifications. We$\backslash$nevaluate proposals for rule modification by modelling a dynamic$\backslash$nspecification as a metric space, and by considering the effects of$\backslash$naccepting a proposal on system utility. Furthermore, we constrain the$\backslash$nenactment of proposals that do not meet the evaluation criteria. We$\backslash$nemploy the action language C+ to formalize dynamic specifications, and$\backslash$nthe `Causal Calculator' implementation of C+ to execute the$\backslash$nspecifications. We illustrate our infrastructure by presenting a dynamic$\backslash$nspecification of a resource-sharing protocol.},
 author = {Artikis, Alexander},
 doi = {10.1093/logcom/exr018},
 file = {/Volumes/DOCUMENTS/MEGA/Mendeley/2012/Artikis/Artikis_2012_Dynamic specification of open agent systems.pdf},
 hashes = {4cdf9afd7788859e7cd01fc0f94fc35146a5c9252aca2bc4c8647d6ba11699bb},
 issn = {0955792X},
 journal = {Journal of Logic and Computation},
 number = {6},
 pages = {1301--1334},
 tags = {executable specification,organized adaptation,agent,MAS,institution,rule,game,theory,meta,taxonomy,deontic,norm change,calculus,sanction,norm,norm-governed system},
 title = {Dynamic specification of open agent systems},
 volume = {22},
