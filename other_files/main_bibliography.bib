 issn = {0011-3921},
 journal = {Current Sociology},
 number = {7},
 pages = {1036--1053},
 tags = {honour killings,femicide,agent,authority,institution,rule,theory,phenomena,female homicide victimization,bourdieu,violence against women,immigrants,symbolic violence,norm},
 title = {An explanation of honour-related killings of women in Europe through Bourdieu's concept of symbolic violence and masculine domination},
