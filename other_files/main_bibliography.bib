 hashes = {105de12dd17764b59203e8ffb8440ed7e012a262e5aa84e51b4042606bde3952;7b6e2eb2016136ddeee51da394efe37158b61eb13f6920a0bb87151fddecc955},
 journal = {Technical Report, UCSC-SOE-11},
