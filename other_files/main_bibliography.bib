 pages = {77},
 pmid = {435310},
 publisher = {Johns Hopkins University Press},
 tags = {agent,authority,institution,rule,game,typology,theory,meta,phenomena,bourdieu,ritual,sanction,norm},
 title = {Cybertext: Perspectives on Ergodic Literature},
