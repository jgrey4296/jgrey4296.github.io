 journal = {American Sociological Review},
 number = {1},
 pages = {101--112},
 tags = {sociology, news, behaviour, scandal, journalism, routine, public},
 title = {News as Purposive Behavior: On the Strategic Use of Routine Events, Accidents, and Scandals},
 volume = {39},
 year = {1974}
