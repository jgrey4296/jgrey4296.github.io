 eprint = {1610.07432},
 file = {/Volumes/DOCUMENTS/MEGA/Mendeley/2016/Kiela et al/Kiela et al._2016_Virtual Embodiment A Scalable Long-Term Strategy for Artificial Intelligence Research.pdf},
 hashes = {3d58f7cfaf99f4a35dd01ae1d8f027e87e8967ae26f302a48fc06a867754b192},
 number = {Nips},
 tags = {agent,MAS,theory,arxiv,game},
 title = {Virtual Embodiment: A Scalable Long-Term Strategy for Artificial Intelligence Research},
