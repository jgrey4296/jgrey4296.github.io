 issn = {15299732 (ISSN)},
 journal = {Journal of Trauma and Dissociation},
 number = {4},
 pages = {37--58},
 tags = {Borderline,theory,rule,Sadism,Abuse,phenomena,Dissociation,Masochism,authority,Attachment},
 title = {Masochistic and sadistic ego states: Dissociative solutions to the dilemma of attachment to an abusive caretaker},
 url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-0012074560&doi=10.1300%2FJ229v02n04_03&partnerID=40&md5=d02226c3a338f3893643dee081724fb3%0Ahttps://www.scopus.com/inward/record.uri?eid=2-s2.0-79959290112&doi=10.1300%2FJ130v02n04_03&partnerID=40&md5=bfe},
