 isbn = {1943-068X VO - 1},
 issn = {1943068X},
 journal = {IEEE Transactions on Computational Intelligence and AI in Games},
 number = {4},
 pages = {245--263},
 tags = {Neuroevolution of augmenting topologies (NEAT),Particle systems,agent,Collaborative content generation,rule,game,mateas,Galactic arms race (GAR),Interactive evolutionary computation (IEC),Collaborative content evolution (CCE),phenomena,Papers,publicationless,Content-generating neuroevolution of augmenting to},
