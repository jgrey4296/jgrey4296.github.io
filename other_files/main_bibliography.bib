@book{Tummolini2013,
 abstract = {The aim of this paper is to clarify what kind of normativity$\backslash$ncharacterizes a convention. First, we argue that conventions have$\backslash$nnormative consequences because they always involve a form of trust and$\backslash$nreliance. We contend that it is by reference to a moral principle$\backslash$nimpinging on these aspects (i.e. the principle of Reliability) that$\backslash$ninterpersonal obligations and rights originate from conventional$\backslash$nregularities. Second, we argue that the system of mutual expectations$\backslash$npresupposed by conventions is a source of agreements. Agreements$\backslash$nstemming from conventions are ``tacit'́\ in the sense that they are$\backslash$nimplicated by what agents do (or forbear from doing) and without that$\backslash$nany communication between them is necessary. To justify this conclusion,$\backslash$nwe assume that: (1) there is a salient interpretation, in some contexts,$\backslash$nof everyone's silence as confirmatory of the others' expectations (an$\backslash$nepistemic assumption), and (2) the participating agents share a value of$\backslash$nnot being motivated by hostile attitudes (a motivational assumption). By$\backslash$nclarifying the relation between conventions and agreements, the peculiar$\backslash$nnormativity of conventions is analyzed.},
 author = {Tummolini, Luca and Andrighetto, Giulia and Castelfranchi, Cristiano and Conte, Rosaria},
 booktitle = {Synthese},
 doi = {10.1007/s11229-012-0194-8},
