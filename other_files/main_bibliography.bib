 issn = {11741740},
 journal = {AlterNative},
 number = {1},
 pages = {25--34},
 tags = {inequity,colonisation,indigenous,privilege,historical trauma,racism},
 title = {Conceptualising historical privilege: the flip side of historical trauma, a brief examination},
 volume = {14},
 year = {2018}
}

