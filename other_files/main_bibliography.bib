 publisher = {Elsevier B.V.},
 tags = {Goal-directed behaviour,lit_review,Planning,survey,agent,MAS,theory,rule,meta,Motivation,taxonomy,Agent architecture},
 title = {A survey of motivation frameworks for intelligent systems},
