 isbn = {9780511530104},
 journal = {Futures},
 number = {September},
 pages = {162},
 tags = {theory,rule,calculus,haskell,game},
 title = {Purely Functional Data Structures Thesis Committee},
 url = {http://ebooks.cambridge.org/ref/id/CBO9780511530104},
 volume = {33},
 year = {1996}
