 isbn = {8480042699},
 journal = {Rule-Based Expert Systems The MYCIN Experiments of the Stanford Heuristic Programming Project MYCIN Experiments of the Stanford Heuristic Programming Project},
 number = {1943},
 pages = {20--54},
