 isbn = {18792782},
 issn = {08936080},
 journal = {Neural Networks},
 number = {301},
 pages = {70--83},
 pmid = {21803542},
 tags = {Sequential retrieval,Long Short Term Memory (LSTM),theory,rule,Temporal sequence processing,Gradient-based training,Recurrent neural network},
