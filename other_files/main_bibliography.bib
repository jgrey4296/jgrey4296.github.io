 issn = {09258531},
 journal = {Journal of Logic, Language and Information},
 number = {3},
 pages = {327--351},
 tags = {deontic,action,Coalition logic,Game theory,logic},
 title = {A dynamic logic of agency II: Deterministic DLA, coalition logic, and game theory},
 volume = {19},
 year = {2010}
}

