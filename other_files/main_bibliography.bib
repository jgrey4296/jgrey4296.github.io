@article{Fabiola2004,
 abstract = {One of the key issues in the computational representation of open societies relates to the introduction$\backslash$r$\backslash$nof norms that help to cope with the heterogeneity, the autonomy and the diversity of interests among$\backslash$r$\backslash$ntheir members. Research regarding this issue presents two omissions. One is the lack of a canonical$\backslash$r$\backslash$nmodel of norms that facilitates their implementation, and that allows us to describe the processes of$\backslash$r$\backslash$nreasoning about norms. The other refers to considering, in the model of normative multi-agent systems,$\backslash$r$\backslash$nthe perspective of individual agents and what they might need to effectively reason about the society$\backslash$r$\backslash$nin which they participate. Both are the concerns of this paper, and the main objective is to present a$\backslash$r$\backslash$nformal normative framework for agent-based systems.},
