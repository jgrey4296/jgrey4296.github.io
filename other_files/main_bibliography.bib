@article{Murphie2013,
 abstract = {In contemporary performance, "action" seems to include acts, emergent action and the potential for action - even sometimes non-action. Performers include the human and nonhuman, the living and the nonliving. Through all this runs a complexity of technics - technologies and techniques - and these increasingly involve computational processes. How do we think the last of these - computational processes in performance? [2] This article gives some answers to this question by engaging with four different but overlapping examples of the way that computational processes are brought into performance: the digital signal processing techique of convolution, live with the collaborative network performance softwares, keyworx. It suggests the necessity of understanding the imperceptible aspects of computaional processes, such as signal processing, as performing, even if not always directy presented. This in turn suggests a more subtle understanding of the micro-dynamics of perfomance in general. As part of this, i propose that more attention is paid to the transformation of signals of all kinds, in and perhaps even as performance. The article concludes by proposing that we now live, at least in part, in a culture of ongoing convolution of signals. Ghis is a culture more sefl-consciously immersed in the ongoing differentation (and integration) of intensities. The article therefore raises questions about the general perfomativity of a culture and technics increasingly enmeshed with computational processes.},
 author = {Murphie, Andrew},
 file = {/Volumes/DOCUMENTS/MEGA/Mendeley/2013/Murphie/Murphie_2013_Convolving Signals Thinking the performance of computational processes.pdf},
 hashes = {e97112eb295640a63e8fa5f1b45292438df4bf67df2d4b6659aef2391de0a6dc},
 issn = {1832-5580},
 journal = {Performance Paradigm},
 number = {9},
 pages = {1--21},
 tags = {theory,phenomena},
 title = {Convolving Signals: Thinking the performance of computational processes},
 url = {https://www.academia.edu/5890757/Convolving_Signals_Thinking_the_performance_of_computational_processes},
 year = {2013}
}

