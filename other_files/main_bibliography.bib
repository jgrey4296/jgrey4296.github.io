 file = {/Volumes/DOCUMENTS/MEGA/Mendeley/2018/Testa, Notarnicola, Notarstefano/Testa, Notarnicola, Notarstefano_2018_Distributed Submodular Minimization over Networks a Greedy Column Generation Approach.pdf},
 hashes = {e41619336f644f18cb25fb5780a77c56d79d37370f5a77c3751ed60ca718d5c5},
