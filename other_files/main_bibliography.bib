 isbn = {1461-3808},
 issn = {1461-3808},
 journal = {Music Education Research},
 month = {sep},
 number = {3},
 pages = {361--372},
 tags = {institution,theory,identity formation,music education,phenomena,musikdidaktik,phenomenology,Papers},
 title = {Playing to teach music – embodiment and identity-making in musikdidaktik},
 url = {http://www.tandfonline.com/doi/abs/10.1080/14613800802280100 http://www.ingentaconnect.com/content/routledg/cmue/2008/00000010/00000003/art00003},
