 issn = {02773791},
 journal = {Quaternary Science Reviews},
 pages = {13--36},
 publisher = {Elsevier Ltd},
 tags = {South America,Great dying,Land use change,Archaeology,Disease epidemics,Anthropocene,Carbon cycle dynamics,De-population,Vegetation dynamics,Central America},
 title = {Earth system impacts of the European arrival and Great Dying in the Americas after 1492},
