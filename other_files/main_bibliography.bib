 journal = {Lecture Notes in Computer Science (including subseries Lecture Notes in Artificial Intelligence and Lecture Notes in Bioinformatics)},
 pages = {224--241},
 tags = {Human-agent collaboration,Joint action,MAS,agent,authority,institution,rule,typology,delegation,dagstuhl,theory,meta,phenomena,Interdependence,BlocksWorld for teams,calculus,sanction,norm,deontic},
