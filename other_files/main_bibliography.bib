 isbn = {9781412931236},
 issn = {9781412931236},
 journal = {the Sage Handbook of Organizational Institutionalism},
 pages = {788--809},
 pmid = {25246403},
 tags = {agent,authority,institution,rule,game,delegation,theory,phenomena,ritual,norm},
 title = {Reflections on Institutional Theories of Organizations},
 year = {2006}
}

