 isbn = {1943-068X},
 issn = {1943068X},
 journal = {IEEE Transactions on Computational Intelligence and AI in Games},
 number = {4},
 pages = {317--335},
 pmid = {8356185},
 tags = {agent,MAS,Artificial intelligence,theory,rule,game,mateas,dagstuhl,arxiv,computational intelligence,taxonomy,meta,games},
