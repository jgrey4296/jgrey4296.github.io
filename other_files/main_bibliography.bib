 tags = {netlogo,abms,strategies,Agent based modeling and simulation (ABMS),agent,rat,q,routine activity theory,rule,theory,learning,crime simulation,police patrolling,phenomena,Routin,agent based modeling and,simulation},
 title = {Agent Based Simulation of Routine Activity Theory},
 volume = {3},
 year = {2014}
}

