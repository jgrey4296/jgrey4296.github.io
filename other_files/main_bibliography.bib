@inproceedings{Balas2004,
 abstract = {Petri Nets can be used for a retrospective analysis of a computer game$\backslash$nstory, for representing plots in serious games as well as for$\backslash$nmonitoring the course of the story, as recently demonstrated by several$\backslash$nauthors. It was not clear, however, whether Petri Nets can be used for$\backslash$nrepresenting plots and for their unfolding in games that feature large$\backslash$nworlds inhabited by virtual humans. This paper demonstrates that this$\backslash$nis indeed possible, presenting several scenarios from the serious game$\backslash$nKaro, an ongoing project, which features both virtual humans driven by$\backslash$nreactive planning and a story manager unfolding story plots represented$\backslash$nand controlled by Petri Nets. However, a specific refinement of Petri$\backslash$nNets, a hierarchical model capitalising on Timed Coloured Petri Nets$\backslash$nand non-deterministic FSMs, had to be developed for this purpose. This$\backslash$nrefinement is described here, including general discussion showing why$\backslash$nindividual features of this refinement are needed - directions for game$\backslash$ndevelopers considering whether to use this technique, or not.},
 author = {Balas, Daniel and Brom, Cyril and Abonyi, Adam and Gemrot, Jakub},
