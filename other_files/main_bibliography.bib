 hashes = {af7a0b689c94f7fbd80afde02352870a42d006e9c404337520704285857b34dc},
 issn = {10902139},
 journal = {Brain, Behavior, and Immunity},
 number = {January},
 pages = {0--1},
 publisher = {Elsevier},
 tags = {AMPA,Rat,Addiction,Tumor necrosis factor,Glutamate,Maternal separation,Cytokine,Conditioned place preference,Adolescent},
 title = {Effects of early life stress on cocaine conditioning and AMPA receptor composition are sex-specific and driven by TNF},
