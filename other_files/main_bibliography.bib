 hashes = {481c3bd35db6bf3b7d2946a20ba1f49487f25b49beede68f97651da9ca35227f},
 isbn = {9810218109},
 issn = {00930415},
 journal = {Proceedings of the Third Annual Conference on Evolutionary Programming},
 number = {5},
 pages = {131--139},
 pmid = {10832428},
 tags = {cultural_algorithms, culture, mappa, genetic_algorithms, lattices},
 title = {An introduction to cultural algorithms},
 url = {http://www.ncbi.nlm.nih.gov/pubmed/10832428%5Cnhttp://www.danielcondurachi.ro/podpress_trac/feed/10/0/cult94.pdf},
 volume = {172},
 year = {1994}
