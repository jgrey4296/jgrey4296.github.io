 journal = {Ssrn},
 number = {August 2015},
 tags = {happiness,income comparisons,self-image,disclosure policy,D03,relative income,life satisfaction,social-image,D31,D80,D60},
 title = {The Effects of Income Transparency on Well-Being: Evidence from a Natural Experiment},
 year = {2015}
