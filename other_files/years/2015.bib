 tags = {H.5.2 [Information Interfaces and Presentation]: U,H.3.3 [Information Storage and Retrieval]: Informa},
 title = {Refinery: Visual Exploration of Large, Heterogeneous Networks through Associative Browsing},
 volume = {34},
