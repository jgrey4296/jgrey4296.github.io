@article{Pitt2015,
 abstract = {Many open networks, distributed computing systems, and infrastructure management systems face a common problem: how to distribute a collectivised set of resources amongst a set of autonomous agents of heterogenous provenance. One approach is for the agents themselves to self-organise the allocation of resources with respect to a set of agreed conventional rules; but given an allocation scheme which maps resources to those agents and a set of rules for determining that allocation scheme, some natural questions arise-Is this allocation fair? Is the allocation method effective? Is it efficient? Are the decision makers accountable? In this paper, we argue that some answers to these questions can be found in the formal characterisation of different aspects of 'justice' and that these different aspects need a principled operationalisation as policies for system management. We present a formal model and some experimental results, concluding that the different aspects are all inter-connected and that what is required is a comprehensive research programme in computational justice. © 2013 Springer-Verlag London.},
 author = {Pitt, Jeremy and Busquets, Dídac and Riveret, Régis},
 doi = {10.1007/s00146-013-0531-6},
 file = {/Volumes/DOCUMENTS/MEGA/Mendeley/2015/Pitt, Busquets, Riveret/Pitt, Busquets, Riveret_2015_The pursuit of computational justice in open systems.pdf;/Volumes/DOCUMENTS/MEGA/Mendeley/2015/Pitt, Busquets, Riveret/Pitt, Busquets, Riveret_2015_The pursuit of computational justice in open systems(2).pdf},
 hashes = {cfe6adb82cd897353bb85badb4498bb58a7a27a3d22b478fbe32e5827c284608;e3c82aa3443c4380af91da801d2726024e23523392bb88a6c09fbe2218533a78},
 issn = {14355655},
 journal = {AI and Society},
