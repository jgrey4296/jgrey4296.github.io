 journal = {Systems Modeling},
 number = {1},
 pages = {1--14},
 tags = {economics, complex_systms, ABM, evolution, culture, game_theory},
 title = {Human development dynamics: an agent based simulation of macro social systems and individual heterogeneous evolutionary games},
 url = {http://www.casmodeling.com/content/1/1/18/%5Cnhttp://pdxscholar.library.pdx.edu/polisci_fac/34},
