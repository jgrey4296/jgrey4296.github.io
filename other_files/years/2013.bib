 hashes = {0e176e94b12767cc8caaa3d8414f864066269aea533c88ac88e90da598871a9e},
 issn = {1664-1078},
 month = {jan},
 number = {May},
 pages = {214},
 pmid = {23641224},
 tags = {embodiment, social, tools, artifacts, words},
 title = {The embodied mind extended: using words as social tools.},
 url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3640182&tool=pmcentrez&rendertype=abstract},
 volume = {4},
 year = {2013}
